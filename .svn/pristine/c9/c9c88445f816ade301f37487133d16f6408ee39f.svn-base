$(function(){
	var supIDParam = $("#supIDParam").val();
	var strArseaLebelOne = $("#strArseaLebelOne").val();
	var strAreaLebelTwo = $("#strAreaLebelTwo").val();
	var strAreaLebelThree = $("#strAreaLebelThree").val();
	var strAreaLebelFour = $("#strAreaLebelFour").val();
	var url="/AWeb/management/address/loadData";
	$.getJSON(url,function (data) {
        $(data).each(function () {
        	$(this.supInfos).each(function () {
        		if(supIDParam == this.supID){
        			$("#supID").append($("<option/>").text(this.supName).attr("value",this.supID).attr("selected", true));
        		}else{
        			$("#supID").append($("<option/>").text(this.supName).attr("value",this.supID));
        		}
            	
                
            });
        	$(this.areas).each(function () {
        		if(strArseaLebelOne == this.supAreaName){
        			alert(strArseaLebelOne);
        			$("#arseaLebelSelOne").append($("<option/>").text(this.supAreaName).attr("value",this.supAreaID).attr("selected", true));
        			$("#arseaLebelOne").val(strArseaLebelOne);
        		}else{
        			$("#arseaLebelSelOne").append($("<option/>").text(this.supAreaName).attr("value",this.supAreaID));
        		}
            });
        });
    });
	
	if(strAreaLebelTwo!=""){
		$("#areaLebelSelTwo").append($("<option/>").text(strAreaLebelTwo).attr("value",strAreaLebelTwo).attr("selected", true));
		$("#areaLebelTwo").val(strAreaLebelTwo);
	}
	if(strAreaLebelThree!=""){
		$("#areaLebelSelThree").append($("<option/>").text(strAreaLebelTwo).attr("value",strAreaLebelTwo).attr("selected", true));
		$("#areaLebelThree").val(strAreaLebelThree);
	}
	if(strAreaLebelFour!=""){
		$("#areaLebelSelFour").append($("<option/>").text(strAreaLebelTwo).attr("value",strAreaLebelTwo).attr("selected", true));
		$("#areaLevelFour").val(strAreaLebelFour);
	}
});

//省级联市
function checkCity(){
        $("#areaLebelSelTwo").empty();
        $("#arseaLebelOne").val($("#arseaLebelSelOne")[0].options[$("#arseaLebelSelOne")[0].selectedIndex].text);
        var data = $("#arseaLebelSelOne").val();
        var url="/AWeb/management/address/loadSubArea?parentId="+data;
        $("#areaLebelSelTwo").append($("<option/>").text("请选择").attr("value",""));
        $.getJSON(url,function (data) {
            $(data).each(function () {
                $("#areaLebelSelTwo").append($("<option/>").text(this.supAreaName).attr("value",this.supAreaID));
            });
            checkDistrict();
        });
}
//市级联区
function checkDistrict(){
	    $("#areaLebelSelThree").empty();
	    $("#areaLebelTwo").val($("#areaLebelSelTwo")[0].options[$("#areaLebelSelTwo")[0].selectedIndex].text);
        var data = $("#areaLebelSelTwo").val();
        var url="/AWeb/management/address/loadSubArea?parentId="+data;
	    $("#areaLebelSelThree").append($("<option/>").text("请选择").attr("value",""));
	    $.getJSON(url,function (data) {
	        $(data).each(function () {
	            $("#areaLebelSelThree").append($("<option/>").text(this.supAreaName).attr("value",this.supAreaID));
	        });
	    });
}
//市级联区
function checkAreaLebelSelThree(){
	$("#areaLebelThree").val($("#areaLebelSelThree")[0].options[$("#areaLebelSelThree")[0].selectedIndex].text);
}
function cancle(){
	window.location.href="/AWeb/management/address";
}

function editMarketAddress(type){
	if($("#name").val()==""||$("#name").val()==null){
		alert("请填写收货人姓名！");
		return;
	}
	if($("#supID").val()==""){
		alert("请选择供应商！");
		return;
	}
	if($("#arseaLebelSelOne").val()==""
		||$("#arseaLebelSelTwo").val()==""
		||$("#arseaLebelSelThree").val()==""){
		alert("请完整选择所在区域！");
		return;
	}
	if($("#addressDetail").val()==""||$("#addressDetail").val()==null){
		alert("请填写详细地址！");
		return;
	}
	/*if($("#phone").val()==""||$("#phone").val()==null){
		alert("请填写电话！");
		return;
	}*/
	if($("#ifDefaultCB").is(':checked') == true){
		$("#ifDefault").val("1");
	}else{
		$("#ifDefault").val("0");
	}
	if(type==1){
		document.getElementById("marketAddressForm").action="/AWeb/management/address/add";
		document.getElementById("marketAddressForm").submit();
	}else{
		var updAddressID = $("#updAddressID").val();
		document.getElementById("marketAddressForm").action="/AWeb/management/address/update?addressID="+updAddressID;
		document.getElementById("marketAddressForm").submit();
	}
	
}

