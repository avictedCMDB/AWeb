<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
<title>首页|中航招标网</title>
<link href="css/bootstrap.css" rel="stylesheet"/>
<link href="css/common.css" rel="stylesheet"/>
<link href="css/supervise_recommend.css" rel="stylesheet"/>
<link href="fonts/font-awesome/css/font-awesome.css" rel="stylesheet"/>
<!-- Fontello CSS -->
<link href="fonts/fontello/css/fontello.css" rel="stylesheet"/>
<script type="text/javascript" src="js/jquery.js"></script>
<script type="text/javascript" src="js/rollover.js"></script>
</head>

<body>
<div id="vat_info">
  <div class="inner">
    <div class="add_title clearfix">推荐时间<a href="#"><i class="fa fa-close"></i></a></div>
    <div class="main_inner">
      <div class="up_area clearfix">
        <dl class="clearfix date_list">
              <dt>推荐时间：</dt>
              <dd>
                <ul class="clearfix">
                  <li class="input_box">
                    <div class="input-group">
                      <input type="text" class="form-control special_input"/>
                      <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                    </div>
                  </li>
                  <li>至</li>
                  <li class="input_box">
                    <div class="input-group">
                      <input type="text" class="form-control special_input"/>
                      <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                    </div>
                  </li>
                  
                </ul>
              </dd>
            </dl>
            <dl>
	      	<dt>上传图片：</dt>
	      	<dd>
              <input id="lefile" type="file" style="display:none"/>
              <div class="input-append"> 
                <input id="photoCover" class="input-large form-control" type="text" style="height:28px; width:370px;"/> 
                <a class="btn btn-default red" onclick="$('input[id=lefile]').click();" style="height:28px; line-height:28px; padding-top:0px;padding-bottom:0px; border:none;">浏览文件</a>
                <button type="button" class="btn btn-default blue">开始上传</button>
              </div>
              <script type="text/javascript"> 
			    $('input[id=lefile]').change(function() { 
				  $('#photoCover').val($(this).val()); 
				});
		      </script>
              <p class="attention">XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX</p>
              <div class="img_list">
                  <p class="img"><img src="img/profession/logo01.gif" /></p>
                  <p><a href="#"><img src="img/icon-close_blue.png" /></a></p>
              </div>
           </dd>
      	</dl>
        </div>
      <div class="btn_list">
          <button class="btn btn-default blue">保存</button>
      </div>
    </div>
  </div>
</div>


</body>
</html>