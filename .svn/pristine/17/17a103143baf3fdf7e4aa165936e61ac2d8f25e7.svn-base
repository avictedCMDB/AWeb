@charset "utf-8";
/* CSS Document */
#matter_section .search_area {
  background: #f6f4f5;
  padding: 25px 0 15px 15px;
  border: 1px solid #dfdfdf;
}
#matter_section .search_area dl {
	float: left;
	font-size: 12px;
	margin-bottom: 10px;
	margin-right: 30px;
}
#matter_section .search_area dl.last {
	margin-right: 0;
}
#matter_section .search_area dl dt {
	float: left;
	line-height: 25px;
	margin-right: 5px;
	width: 65px;
	text-align: right;
}
#matter_section .search_area dl dd {
	float: left;
}
#matter_section ul li {
	float: left;
	line-height: 25px;
	margin-right: 8px;
}
#matter_section .special_input {
	border-right: none;
}
#matter_section .form-control {
	height: 28px;
	width: 215px;
}
#matter_section .input-group-addon {
	height: 28px;
	padding: 7px 5px 5px 0px;
	border-left: none;
	background: #fff;
	cursor: pointer;
	display: inline-table;
}
.btn {
	height: 28px;
	line-height: 28px;
	padding: 0 20px;
	border: none;
}
.red {
	background: #0091dc;
	color: #fff;
}
.gray {
	background: #666666;
	color: #fff;
}
.btn_list {
	text-align: right;
}
.btn_list ul {
	display: inline-block;
}
.search_result {
	padding-top: 15px;
}
.state_list {
	margin-bottom: 15px;
	overflow: hidden;
}
.state_list ul li a {
	display: block;
	width: 75px;
	text-align: center;
	border: 1px solid #dfdfdf;
}
.state_list ul li.active a,
.state_list ul li a:hover {
	background: #0091dc;
	color: #fff;
}


.search_result table {
	width: 950px;
}
.search_result table tr th {
	background: #dfdfdf;
	border:1px solid #dfdfdf;
	border-right: 1px solid #fff;
	line-height: 32px;
	text-align: center;
	font-size: 12px;
}
.search_result table tr th.last {
	border-right: 1px solid #dfdfdf;
}
.search_result table tr td {
	border:1px solid #dfdfdf;
	padding: 10px;
	font-size: 12px;
	vertical-align: middle;
	text-align: center;
}
.search_result table tr td a {
	word-break: break-all;
}
.search_result table tr td.right_none {
	border-right: none;
}
.search_result table tr td.left_none {
	border-left: none;
	text-align: left;
	color: #4798ed;
}
.search_result table tr.company td {
	border: none;
	font-weight: bold;
}
.search_result table tr td a {
	margin-right: 5px;
	color: #4798ed;
}
.search_result table tr td a:hover {
	text-decoration: underline;
}
.search_result table tr.company td span{
	font-weight: 100;
}
.img_pro {
	border:1px solid #dfdfdf;
	margin-bottom: 0;
}
.price {
	color: #ff4200;
}
.search_result table tr td.price,
.search_result table tr td.num {
	vertical-align: middle;
}
#matter_section .search_result table tr.company td ul li.right_box {
	float: right;
	margin-left: 30px;
}
.inner h3 {
  border-left: 5px solid #0091dc;
  font-size: 16px;
  padding-left: 10px;
  color: #0091dc;
  font-weight: bold;
  margin-bottom: 0;
}
table.details {
	width: 100%;
	margin-top: 0;
}
.search_result table tr.sum_price td {
	text-align: right;
	color: #ff4200;
	border:none;
}
.search_result table tr td.right_none {
	border-right: none;
}
.search_result table tr td.left_none {
	border-left: none;
}
.list_section  {
	margin-top: 30px;
	border-bottom: 1px solid #dfdfdf;
}
.list_section dl {
	margin-bottom: 10px;}
.btn_area {
	text-align: center;
	margin: 30px 0;
}

/*订单查看*/


#order_details {
	background-color: #fff;
	font-size: 12px;
	padding-bottom: 20px;
}
#order_details .section_main {
	margin-bottom: 30px;
}

#order_details .section_main h2 {
	border-left: 5px solid #0091dc;
    color: #0091dc;
    font-size: 16px;
    font-weight: bold;
    margin-bottom: 15px;
    padding-left: 10px;
	margin-top: 0;
	border-bottom: none;
	text-align: left;
}
#order_details table {
	width: 100%;
	border: 1px solid #dfdfdf;

}
#order_details table tr td {
	padding: 10px;
}
#order_details table tr th {
	background: #dfdfdf none repeat scroll 0 0;
	border: 1px solid #dfdf;
    line-height: 32px;
    text-align: center;
}
#order_details table tr td {
	text-align: center;
}
#order_details table tr td a {
	color:#008ad3;
}
#order_details table tr td a:hover {
	text-decoration: underline;
}
#order_details table tr td.left_align {
	text-align: left;
}
#order_details .input-group {
	width: 50%;
	margin: 0 auto;
}
#order_details .input-group-addon {
	height: 26px;
	line-height: 26px;
	padding: 0 10px;
	border-radius: 0;
	border-color: #e5e5e5;
	width: 5%;
}
#order_details table tr td .form-control {
	height: 28px;
	width: 40%;
}
#order_details .special_area {
	width: 370px;
	float: left;
	margin: 0 10px;
}
#order_details .first {
	margin-left: 0;
}
#order_details .section_inner {
	background: url(../img/order/bg-list-details.gif) left top repeat-y;
}
#order_details .section_main dl {
	margin-bottom: 10px;
}
#order_details .section_main dl dt {
	width: 98px;
}
#order_details .section_main dl dd {
	margin-left: 98px;
	width: 265px;
}
#order_details .price_area {
	border-top: 3px solid #0091dc;
	padding-top: 20px;
}
#order_details .gather_box {
	float: right;
	width: 422px;
	border: 1px solid #dfdfdf;
	background: #f1f1f1;
}
#order_details .gather_inner {

	padding: 10px 10px 5px 10px;
	text-align: right;
}
#order_details .gather_inner p {
	margin-bottom: 6px;
}
#order_details .gather_inner span {
	font-weight: bold;
	padding-right: 5px;
	line-height: 1.2;
}
#order_details .gather_inner span.symbol {
	font-size: 28px;
	color: #979b9a;
}
#order_details .gather_inner span.price {
	font-size: 28px;
	color: #ff4200;
}
#order_details .btn_list {
	text-align: center;
}
#order_details .btn_list ul {
	display: inline-block;
}
#order_details span.num {
	color: #db433d;
}
#order_details span.img {
	margin-right: 10px;
}
#order_details p.success {
	font-size: 14px;
	line-height: 30px;
	text-align: center;
	border-bottom: 1px solid #dfdfdf;
	padding-bottom: 10px;
}
#order_details .gray {
	color: #fff;
	padding: 0 20px;
	height: 28px;
}


#order_details .input-group-num {
	background: #fcfcfc;
	padding: 5px;
	width: 70%;
}
#order_details .input-group-num .btn {
	width: 20px;
	height: 26px;
	line-height: 26px;
	display: block;
}
#order_details .input-group-num .form-control {
	height: 26px;
	text-align: center;
	width: 100%;
}
#order_details .btn {
    border: medium none;
    font-size: 12px;
    height: 24px;
    line-height: 24px;
    padding: 0;
    text-align: center;
    width: 70px;
	margin-left: 0;
}
#order_details .input-group-num .form-control {
	padding: 0;
	line-height: 26px;
	font-size: 12px;
}
#order_details .btn_list .btn {
	height: 28px;
	font-size: 14px;
}