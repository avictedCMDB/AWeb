@charset "utf-8";
/* CSS Document */
#matter_section .search_area {
  background: #f6f4f5;
  padding: 25px 0 15px 15px;
  border: 1px solid #dfdfdf;
}
#matter_section .search_area dl {
	float: left;
	font-size: 12px;
	margin-bottom: 10px;
	margin-right: 30px;
}
#matter_section .search_area dl.last {
	margin-right: 0;
}
#matter_section .search_area dl dt {
	float: left;
	line-height: 25px;
	margin-right: 5px;
	width: 65px;
	text-align: right;
}
#matter_section .search_area dl dd {
	float: left;
	width: 375px;
}
#matter_section .search_area dl.keyword dd {
	width: 849px;}
#matter_section ul li {
	float: left;
	line-height: 25px;
	margin-right: 8px;
}
#matter_section .form-control {
	height: 28px;
	border-radius: 4px;
}
#matter_section .input-group-btn .btn {
	margin-left: 10px;
	border-radius: 4px;
}
.btn {
	height: 28px;
	line-height: 28px;
	padding: 0 20px;
	border: none;
}
.red {
	background: #db433d;
	color: #fff;
}
.gray {
	background: #666666;
	color: #fff;
}
.btn_list {
	text-align: right;
}
.btn_list ul {
	display: inline-block;
}
.search_result {
	padding-top: 15px;
}
.search_result table {
	width: 950px;
}
.search_result table tr th {
	background: #dfdfdf;
	border:1px solid #dfdfdf;
	border-right: 1px solid #fff;
	line-height: 32px;
	text-align: center;
	font-size: 12px;
}
.search_result table tr th.last {
	border-right: 1px solid #dfdfdf;
}
.search_result table tr td {
	border:1px solid #dfdfdf;
	padding: 10px;
	font-size: 12px;
	vertical-align: middle;
	text-align: center;
}
.search_result table tr td a {
	word-break: break-all;
}
.search_result table tr td a {
	margin-right: 5px;
	color: #4798ed;
}
.search_result table tr td a:hover {
	text-decoration: underline;
}
.search_result table tr td.name {
	text-align: left;
}