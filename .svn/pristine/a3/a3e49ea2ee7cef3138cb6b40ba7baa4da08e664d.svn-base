//市级联区
function checkAreaLebelSelThree(){
	if($("#supID").val()=="1"){
		$("#areaLevelFour").val("");
		$("#areaLebelThree").val($("#areaLebelSelThree")[0].options[$("#areaLebelSelThree")[0].selectedIndex].text=="请选择"?"":$("#areaLebelSelThree")[0].options[$("#areaLebelSelThree")[0].selectedIndex].text);
	}else{
		$("#areaLebelSelFour").empty();
	    $("#areaLebelThree").val($("#areaLebelSelThree")[0].options[$("#areaLebelSelThree")[0].selectedIndex].text=="请选择"?"":$("#areaLebelSelThree")[0].options[$("#areaLebelSelThree")[0].selectedIndex].text);
        var data = $("#areaLebelSelThree").val();
        var url="/AWeb/management/address/loadSubArea?parentId="+data;
	    $("#areaLebelSelFour").append($("<option/>").text("请选择").attr("value",""));
	    $.getJSON(url,function (data) {
	        $(data).each(function () {
	        	if(strAreaLebelFour==this.supAreaName){
	        		$("#areaLebelSelFour").append($("<option/>").text(this.supAreaName).attr("value",this.supAreaID).attr("selected", true));
	        	}else{
	        		$("#areaLebelSelFour").append($("<option/>").text(this.supAreaName).attr("value",this.supAreaID));
	        	}
	            
	        });
	        checkAreaLebelSelFour();
	    });
	    $("#areaLebelSelFour").change(function(){
	    	checkAreaLebelSelFour();
	    });
	    
	}
}

//市级联区
function checkAreaLebelSelFour(){
	$("#areaLevelFour").val($("#areaLebelSelFour")[0].options[$("#areaLebelSelFour")[0].selectedIndex].text=="请选择"?"":$("#areaLebelSelFour")[0].options[$("#areaLebelSelFour")[0].selectedIndex].text);
}
function cancle(){
	window.location.href="/AWeb/management/address";
}

function editMarketAddress(type){
	if($("#name").val()==""||$("#name").val()==null){
		alert("请填写收货人姓名！");
		return;
	}
	if($("#supID").val()==""){
		alert("请选择供应商！");
		return;
	}
	if($("#arseaLebelSelOne").val()==""
		||$("#arseaLebelSelTwo").val()==""
		||$("#arseaLebelSelThree").val()==""){
		alert("请完整选择所在区域！");
		return;
	}
	if($("#addressDetail").val()==""||$("#addressDetail").val()==null){
		alert("请填写详细地址！");
		return;
	}
	/*if($("#phone").val()==""||$("#phone").val()==null){
		alert("请填写电话！");
		return;
	}*/
	if($("#ifDefaultCB").is(':checked') == true){
		$("#ifDefault").val("1");
	}else{
		$("#ifDefault").val("0");
	}
	if(type==1){
		document.getElementById("marketAddressForm").action="/AWeb/management/address/add";
		document.getElementById("marketAddressForm").submit();
	}else{
		var updAddressID = $("#updAddressID").val();
		document.getElementById("marketAddressForm").action="/AWeb/management/address/update?addressID="+updAddressID;
		document.getElementById("marketAddressForm").submit();
	}
	
}

function addAreaSel(){
	$("#areaLebelSelFour").remove();
	if($("#supID").val()=="2"){
		$("#areaDD").append("<select  id='areaLebelSelFour' name='areaLebelSelFour' class='form-control'  onchange='checkAreaLebelSelFour();'><option value='' selected='selected'>请选择</option></select>");
		checkAreaLebelSelThree();
	}else{
		$("#areaLevelFour").val("");
	}
}