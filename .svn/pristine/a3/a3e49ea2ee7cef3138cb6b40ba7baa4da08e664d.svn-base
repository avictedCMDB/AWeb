	    $("#areaLebelSelThree").empty();
	    $("#areaLebelTwo").val($("#areaLebelSelTwo")[0].options[$("#areaLebelSelTwo")[0].selectedIndex].text=="请选择"?"":$("#areaLebelSelTwo")[0].options[$("#areaLebelSelTwo")[0].selectedIndex].text);
        var data = $("#areaLebelSelTwo").val();
        var url="/AWeb/management/address/loadSubArea?parentId="+data;
