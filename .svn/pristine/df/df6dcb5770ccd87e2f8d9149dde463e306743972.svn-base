@charset "utf-8";
/* CSS Document */
#matter_section .search_area dl dt {
	width: 80px;
}
#matter_section .search_area dl dd {
	width: 202px;
}
#matter_section .special_input {
	width: 70px;
}
.search_result table tr.company td {
    border: none;
    font-weight: bold;
}
#matter_section .search_result table tr.company td ul li.right_box {
    float: right;
    margin-left: 30px;
}
.search_result table tr.company td span {
    font-weight: 100;
}
.price {
    color: #db433d;
}
.search_result table tr td.right_none {
    border-right: none;
}
.search_result table tr td.left_none {
    border-left: none;
	text-align: left;
}
.search_result table {
	width: 100%;
	margin-top: 0;
}
.search_result table tr th {
	border: none;
	border-right: none;
	line-height: 37px;
}
.search_result table tr td {
	text-align: center;
  vertical-align: middle; 
}
.search_result table tr.company td {
  border: medium none;
  font-weight: bold;
  text-align: left;
}
.search_result table tr td.pro_name {
  text-align: left;
}
.img_pro {
	border:1px solid #dfdfdf;
	margin-bottom: 0;
}
.search_result table tr td.right_line {
	border-right: 1px solid #dfdfdf;
}
.search_result table tr td.left_line {
	border-left: 1px solid #dfdfdf;
	text-align: left;
}
.search_result table tr td.link_area {
	text-align: left;
	color: #4798ed;
}
.search_result table tr td.icon_area {
	text-align: left;
}
.search_result table tr td a {
	word-break: break-all;	
	color: #0089cf;
}
.search_result table tr td a:hover {
	color: #db433d;
}
.search_result table tr td.icon_area .btn {	
	width: 70px;
	height: 24px;
	line-height: 24px;
	font-size: 12px;
	padding: 0;
	border: none;
}

/*---------------- #product_detail --------------------*/
.breadcrumb {
	padding: 8px 0;
}

.breadcrumb li {
	float: left;
	padding-right: 10px;
	font-size: 12px;
}

.breadcrumb li span {
	padding-left: 10px;
}

#preview {
	margin-right: 27px;
	float: left;
	width: 360px;
}

#preview .photo {
	border: 1px solid #dfdfdf;
}

#preview .spec_list {
	margin-top: 17px;
}
#preview .spec_list ul li.arrow_left {
	border: none;
	margin-top: 10px;
	margin-right: 14px;
}
#preview .spec_list ul li.arrow_right {
	border: none;
	margin-top: 10px;
	margin-left: 14px;
	margin-right: 0;
}
#preview .spec_list ul li.last {
	margin-right: 0;
}
#preview .spec_list .arrow_right {
	
}

#preview .spec_list ul {

}

#preview .spec_list li {
	margin-right: 20px;
	float: left;
	border: 1px solid #dfdfdf;
}

#item_info {
	position: relative;
	float: left;
	width: 555px;
	font-size: 12px;
}

#item_info h1 {
	margin: 5px 0 20px 0;
	font-size: 18px;
	font-weight: bold;
}

#summary_price {
	margin-bottom: 0;
	padding: 5px 10px;
    background-color: #f6f4f5;
}

#summary_price dt,
#summary_price dd {
	padding-right: 50px;
	float: left;
}

#summary_price dt {
	padding: 5px 10px 0 0;
}

#summary_price dd {
	padding-right: 50px;
}

#summary_price dd.preferential {
	color: #db4340;
	font-size: 20px;
	font-weight: bold;
}

#summary_price dd.general {
	padding-top: 2px;
	font-size: 18px;
	font-weight: bold;
}

#summary_price dd.general span {
	text-decoration:line-through;
}

#comment_count {
	margin-bottom: 0;
	padding: 10px 10px 9px 428px;
    border-bottom: 1px solid #dfdfdf;
}

#comment_count dt,
#comment_count dd {
	float: left;
	font-weight: normal;
}

#choose {
	margin-top: 6px;
}

#choose dt {
	padding-left: 22px;
	float: left;
	width: 84px;
	line-height: 2.6;
	font-weight: normal;
}

#choose dd {
	float: right;
	width: 471px;
	line-height: 2.6;
}

#choose #quantity {
	position: relative;
	margin-bottom: 0;
	width: 90px;
	line-height: 26px;
	text-align: center;
	border: 1px solid #ccc;
	
}

#choose dd span {
	position: absolute;
	padding: 0 4px;
	font-size: 26px;
	line-height: 1;
	text-align: center;
	width: 25px;
	background-color: #edeef0;
}

#choose dd span.btn_reduce {
	top: 0;
	left: 0;
	color: #cdccca;
	border-right: 1px solid #ccc;
}

#choose dd span.btn_add {
	top: 0;
	right: 0;
	color: #3a3c39;
	border-left: 1px solid #ccc;
}

#btn_list {
	padding-left: 22px;
}

#btn_list li {
	padding-right: 20px;
	float: left;
}

#other {
	padding: 15px 0 0 22px;
}

#other li {
	padding-right: 25px;
	float: left;
	line-height: 17px;
}

#other li img {
	padding-right: 3px;
}

#item_info #compare_map {
	position: absolute;
	top: 0;
	right: 0;
}

#ext_info {
	float: right;
	width: 208px;
	font-size: 12px;
	border: 1px solid #dfdfdf;
}

#ext_info h2 {
	margin: 0;
	padding: 10px;
	font-size: 18px;
	font-weight: bold;
	color: #db4340;
	border-bottom: 1px solid #dfdfdf;
}

#contact {
	margin-bottom: 0;
	padding: 8px 0 8px 48px;
	border-bottom: 1px solid #dfdfdf;
	background: url("../img/product-detail/bg-contact.png") no-repeat 10px center;
}

#other_shop {
	margin-bottom: 0;
	padding: 10px;
}

#other_shop dt {
	margin-bottom: 5px;
}

/*---------------- #product_detail --------------------*/
#product_detail {
	margin-top: 30px;
	position: relative;
	padding-top: 34px;
	border: 1px solid #dedede;
}

#product_detail #nav {
	position: absolute;
	top: -1px;
	left: 0;
	z-index: 100;
}

#product_detail #nav li {
	float: left;
	width: 128px;
	text-align: center;
	line-height: 36px;
	border-right: 1px solid #dedede;
}

#product_detail #nav li a:hover {
	display: block;
	background: url("../img/product-detail/bg-nav_on.gif") no-repeat top center;
}

#product_detail #nav li.select {
	background: url("../img/product-detail/bg-nav_on.gif") no-repeat top center;
}

#tab01,
#tab02,
#tab03 {
	font-size: 12px;
}

#tab01 {
	padding: 56px 56px 26px 56px;
	border-top: 1px solid #dedede;
}

#tab01 h2,
#tab01 ul {
	margin: 0 auto;
	width: 790px;
}

#tab01 h2,
#tab01 li {
	margin-bottom: 30px;
}

#tab02 {
	display: none;
	padding: 56px;
	border-top: 1px solid #dedede;
}

#tab02 .tableStyle01 {
    margin: 0 auto;
    border: 1px solid #dfdfdf;
    border-collapse: collapse;
    border-spacing: 0;
    empty-cells: show;
    line-height: 1.5;
}

#tab02 .tableStyle01 th {
    border: 1px solid #dfdfdf;
    padding: 5px 10px;
    background: #F2F2F2;
    font-weight: normal;
    text-align: center;
    background-color: #fec1c0;
    font-weight: bold;
}

#tab02 .tableStyle01 td {
    border: 1px solid #dfdfdf;
    padding: 5px 10px;
}

#tab02 .tableStyle01 tr.bg_style {
	background-color: #f6f6f6;
}

#tab03 {
	display: none;
	padding: 56px;
	border-top: 1px solid #dedede;
}

#tab03 .tableStyle02 {
    margin: 0 auto;
    border-collapse: collapse;
    border-spacing: 0;
    empty-cells: show;
    line-height: 1.5;
}

#tab03 .tableStyle02 th {
    border-bottom: 1px solid #dfdfdf;
    padding: 10px;
    background: #F2F2F2;
    font-weight: bold;
    text-align: center;
    background-color: #fff;
}

#tab03 .tableStyle02 td {
    border-bottom: 1px dotted #dfdfdf;
    padding: 10px;
    text-align: center;
}

#tab03 #flip {
	margin-top: 40px;
    padding-left: 632px;
}

#tab03 #flip li {
	margin-right: 3px;
	padding: 5px 15px;
	float: left;
	border: 1px solid #ddd;
	border-radius: 2px;
	background-color: #f5f5f5;
}
#tab03 #flip li.select {
	background-color: #fff;
	border: 0;
}

#compare {
	display: none;
	position: absolute;
	top: -1px;
	left: -20px;
	width: 1206px;
	font-size: 12px;
	border: 2px solid #dc433d;
	z-index: 999;
	background-color: #fff;
}

#compare h2 {
	position: relative;
	margin: 0;
	padding: 10px;
	font-size: 16px;
	font-weight: bold;
	color: #fff;
	background-color: #dc433d;
}

#compare h2 span {
	position: absolute;
	top: 8px;
	right: 8px;
}

#compare_main {
	padding: 15px;
}

#compare_main li {
	padding: 5px 20px;
	float: left;
	border-right: 1px solid #dfdfdf;
}

#compare_main li p {
	float: left;
	margin: 0;
}

#compare_main li p.txt {
	padding-left: 10px;
	width: 140px;
	line-height: 56px;
}

#compare_main li dl {
	margin-bottom: 0;
	padding-top: 14px;
	width: 140px;
	float: right;
}
#compare_main .compare_btn {
	padding: 12px 0 0 70px;
	text-align: center;
	border: none;
}

#compare_main .compare_btn a {
	color: #018ad0;
}
/*订单查看*/


#order_details {
	background-color: #fff;
	font-size: 12px;
	padding-bottom: 20px;
}
#order_details .section_main {
	margin-bottom: 30px;
}

#order_details .section_main h2 {
	border-left: 5px solid #db433d;
    color: #db433d;
    font-size: 16px;
    font-weight: bold;
    margin-bottom: 15px;
    padding-left: 10px;
	margin-top: 0;
	border-bottom: none;
	text-align: left;
}
#order_details table {
	width: 100%;
	border: 1px solid #dfdfdf;

}
#order_details table tr td {
	padding: 10px;
}
#order_details table tr th {
	background: #dfdfdf none repeat scroll 0 0;
	border: 1px solid #dfdf;
    line-height: 32px;
    text-align: center;
}
#order_details table tr td {
	text-align: center;
}
#order_details table tr td a {
	color:#008ad3;
}
#order_details table tr td a:hover {
	text-decoration: underline;
}
#order_details table tr td.left_align {
	text-align: left;
}
#order_details .input-group {
	width: 50%;
	margin: 0 auto;
}
#order_details .input-group-addon {
	height: 26px;
	line-height: 26px;
	padding: 0 10px;
	border-radius: 0;
	border-color: #e5e5e5;
	width: 5%;
}
#order_details table tr td .form-control {
	height: 28px;
	width: 40%;
}
#order_details .special_area {
	width: 370px;
	float: left;
	margin: 0 10px;
}
#order_details .first {
	margin-left: 0;
}
#order_details .section_inner {
	background: url(../img/order/bg-list-details.gif) left top repeat-y;
}
#order_details .section_main dl {
	margin-bottom: 10px;
}
#order_details .section_main dl dt {
	width: 98px;
}
#order_details .section_main dl dd {
	margin-left: 98px;
	width: 265px;
}
#order_details .price_area {
	border-top: 3px solid #db433d;
	padding-top: 20px;
}
#order_details .gather_box {
	float: right;
	width: 422px;
	border: 1px solid #dfdfdf;
	background: #f1f1f1;
}
#order_details .gather_inner {

	padding: 10px 10px 5px 10px;
	text-align: right;
}
#order_details .gather_inner p {
	margin-bottom: 6px;
}
#order_details .gather_inner span {
	font-weight: bold;
	padding-right: 5px;
	line-height: 1.2;
}
#order_details .gather_inner span.symbol {
	font-size: 28px;
	color: #979b9a;
}
#order_details .gather_inner span.price {
	font-size: 28px;
	color: #db433d;
}
#order_details .btn_list {
	text-align: center;
}
#order_details .btn_list ul {
	display: inline-block;
}
#order_details span.num {
	color: #db433d;
}
#order_details span.img {
	margin-right: 10px;
}
#order_details p.success {
	font-size: 14px;
	line-height: 30px;
	text-align: center;
	border-bottom: 1px solid #dfdfdf;
	padding-bottom: 10px;
}
#order_details .gray {
	background-color: #666;
	color: #fff;
	padding: 0 20px;
	height: 28px;
}