            		$("#uploadLicenceFileDiv2").hide();
            		$("#licenceViewerImg2").attr("src", url); 
            		$("#licenceViewerImg2").attr("data-original",url);
            		$("#showLicenceFileDiv2").show();
        		}
        		$("#licenceName").val(picName);
        		$("#licenceFile").val(data);
        		
        	}else if(type=="2"){
        		
        		$("#uploadOrganizationFileDiv").hide();
