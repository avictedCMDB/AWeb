h2,p,h3,h4,h5{
	margin: 0;
}
a {
	color: #0089d1;
}
a:hover {
	color: #ff4200;
}
.inner {
	display: table;
}
#logo {
	width: 600px;
	display: table-cell;
	vertical-align: bottom;
}
#login {
	width: 600px;
	display: table-cell;
	text-align: right;
	vertical-align: bottom;
}
.txt {
	padding-top: 52px;
	display: inline-block;
	padding-left: 30px;
	font-size: 18px;
}
.content {
	margin-top: 20px;
	padding-bottom: 50px;
}
.content .inner {
	background: #fff;
	border: 1px solid #dfdfdf;
	padding:50px;
}
.register_area {
	width: 760px;
	display: table-cell;
	vertical-align: top;
}
.register_list {
	border-right: 1px solid #dfdfdf;
	padding-right: 30px;
}
.register_list dl {
	margin-bottom: 25px;
}
.register_list dl dt {
	width: 100px;
	text-align: right;
	float: left;
	line-height: 34px;
	font-size: 14px;
}
.register_list dl dd {
	width: 350px;
	text-align: left;
	float: left;
	margin-left: 20px;
	font-size: 14px;
	position: relative;
}
.register_list dl dd span.remind {
	position: absolute;
	bottom: -18px;
	left: 0;
	font-size: 12px;
}
.register_list dl dd.role_area {
	width: 580px;
}
.red {
	color: #df4200;
	margin-right: 5px;
}
.role_list ul li {
	float: left;
	margin-right: 20px;
	margin-bottom: 10px;
}
.role_list ul li a {
	display: block;
	width: 120px;
	padding: 8px 0;
	text-align: center;
	border: 1px solid #0089d1;
	color: 
}
.role_list ul li.active a,
.role_list ul li a:hover {
	background: #0089d1;
	color: #fff;
	border: 1px solid #0089d1;
}
.tel_num  {
	font-size: 14px;
	border: 1px solid #ccc;
	border-radius: 4px 0 0 4px;
	padding: 0 10px;
	border-right: none;
}
.tel_num label {
	font-weight: 100;
	padding-top: 5px;
}
.dropdown-menu {
	height: 200px;
	overflow-y:scroll;
}
.btn_register {
	padding-left: 120px;
	margin-top: 30px;
}
.btn_register .btn {
	width: 120px;
	text-align: center;
	line-height: 35px;
	height: 35px;
}
.btn_register .btn.disable {
	background-color: #dfdfdf;
	color: #000;
}
.readbook {
	padding-left: 120px;
	font-size: 14px;
}
.help_area {
	width: 400px;
	padding-left: 30px;
	display: table-cell;
	vertical-align: top;
}
.help_area h2 {
	font-size: 20px;
	margin-bottom: 20px;
}
.help_list ul li {
	margin-bottom: 15px;
	font-size: 14px;
}
.help_list ul li.more {
	padding-left: 25px;
}
.help_list ul li i {
	background: #0089d1;
	color: #fff;
	margin-right: 5px;
	width: 20px;
	height: 20px;
	display: inline-block;
	text-align: center;
	line-height: 20px;
}
.modal-body p{
	font-size: 14px;
	margin-top: 20px;
}
.modal-dialog {
	width: 900px;
}
.modal-body .content_text {
	background: #f4f4f4;
	margin-top: 20px;
	padding: 20px;
}
.modal-body .content_text h3 {
	text-align: center;
	margin-bottom: 20px;
}