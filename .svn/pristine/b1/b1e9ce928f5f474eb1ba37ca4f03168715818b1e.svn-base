@charset "utf-8";
/* CSS Document */
#matter_section ul li {
	float: left;
	line-height: 25px;
	margin-right: 8px;
}
.state_list {
	margin-bottom: 15px;
	text-align: right;
}
.state_list ul {
	display: inline-block;
}
.btn {
	height: 28px;
	line-height: 28px;
	padding: 0 20px;
	border: none;
}
.red {
	background: #db433d;
	color: #fff;
}
.gray {
	background: #666666;
	color: #fff;
}
.search_result table {
	width: 950px;
}
.search_result table tr th {
	background: #dfdfdf;
	border:1px solid #dfdfdf;
	border-right: 1px solid #fff;
	line-height: 32px;
	text-align: center;
	font-size: 12px;
}
.search_result table tr th.last {
	border-right: 1px solid #dfdfdf;
}
.search_result table tr td {
	border:1px solid #dfdfdf;
	padding: 10px;
	font-size: 12px;
	vertical-align: middle;
	text-align: center;
}
.search_result table tr td a {
	word-break: break-all;
}
.search_result table tr td a {
	margin-right: 5px;
	color: #4798ed;
}
.search_result table tr td a:hover {
	text-decoration: underline;
}
.btn_area {
	text-align: center;
	margin: 30px 0;
}
.btn_area ul {
	display: inline-block;
}
.btn_area ul li {
	float: left;
	margin: 0 5px;
}
/*---添加联系人--*/
.return_pro {
	width: 900px;
	margin: 0 auto;
}
.return_pro h2 {
	text-align: center;
	background: #db433d;
	color: #fff;
	font-size: 20px;
	padding: 5px 0;
	margin-top: 0;
	margin-bottom: 20px;
}
.main_list {
	padding-top: 0;
}
.inner_main {
	width: 860px;
	margin: 0 auto;
}
.inner_main .radio,
.inner_main input {
	margin-top: 0;
}
.main_list h3 {
	margin-bottom: 10px;
}
.add_details {
    background: #f6f5f5 none repeat scroll 0 0;
    border: 1px solid #dfdfdf;
    margin: 25px auto 0;
    padding: 30px 75px;
    width: 730px;
}
.add_details dl {
    margin-bottom: 10px;
}
.add_details dt {
    float: left;
    line-height: 28px;
    text-align: right;
    width: 120px;
}
.add_details dd {
	margin-left: 120px;
	line-height: 28px;
}
.add_details span.red_remind {
    color: #db433d;
    display: inline;
    margin-right: 10px;
}
.add_details .form-control {
    height: 28px;
}
.add_details select {
    display: inline-block;
    padding: 0;
    width: 150px;
}


