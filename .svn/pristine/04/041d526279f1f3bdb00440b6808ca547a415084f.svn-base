@charset "utf-8";
/* CSS Document */
#matter_section .search_area {
  background: #f6f4f5;
  padding: 25px 0 15px 15px;
  border: 1px solid #dfdfdf;
}
#matter_section .search_area dl {
	float: left;
	font-size: 12px;
	margin-bottom: 10px;
	margin-right: 30px;
}
#matter_section .search_area dl.last {
	margin-right: 0;
}
#matter_section .search_area dl dt {
	float: left;
	line-height: 25px;
	margin-right: 5px;
	width: 65px;
	text-align: right;
}
#matter_section .search_area dl dd {
	float: left;
	width: 205px;
}
#matter_section ul li {
	float: left;
	line-height: 25px;
	margin-right: 8px;
}
#matter_section .special_input {
	border-right: none;
}
#matter_section .form-control {
	height: 28px;
}
#matter_section .input-group-addon {
	height: 28px;
	padding: 7px 5px 5px 0px;
	border-left: none;
	background: #fff;
	cursor: pointer;
	display: inline-table;
}
.btn {
	height: 28px;
	line-height: 28px;
	padding: 0 20px;
	border: none;
}
.red {
	background: #db433d;
	color: #fff;
}
.gray {
	background: #666666;
	color: #fff;
}
.btn_list {
	text-align: right;
}
.btn_list ul {
	display: inline-block;
}
.search_result {
	padding-top: 15px;
}
.state_list {
	margin-bottom: 15px;
	overflow: hidden;
}
.state_list ul li a {
	display: block;
	width: 75px;
	text-align: center;
	border: 1px solid #dfdfdf;
}
.state_list ul li.active a,
.state_list ul li a:hover {
	background: #db433d;
	color: #fff;
}


.search_result table {
	width: 950px;
}
.search_result table tr th {
	background: #dfdfdf;
	border:1px solid #dfdfdf;
	border-right: 1px solid #fff;
	line-height: 32px;
	text-align: center;
	font-size: 12px;
}
.search_result table tr th.last {
	border-right: 1px solid #dfdfdf;
}
.search_result table tr td {
	border:1px solid #dfdfdf;
	padding: 10px;
	font-size: 12px;
	vertical-align: middle;
	text-align: center;
}
.search_result table tr td a {
	word-break: break-all;
}
.search_result table tr td.right_none {
	border-right: none;
}
.search_result table tr td.left_none {
	border-left: none;
	text-align: left;
	color: #4798ed;
}
.search_result table tr.company td {
	border: none;
	font-weight: bold;
}
.search_result table tr td a {
	margin-right: 5px;
	color: #4798ed;
}
.search_result table tr td a:hover {
	text-decoration: underline;
}
.search_result table tr.company td span{
	font-weight: 100;
}
.img_pro {
	border:1px solid #dfdfdf;
	margin-bottom: 0;
}
.price {
	color: #db433d;
}
.search_result table tr td.price,
.search_result table tr td.num {
	vertical-align: middle;
}
#matter_section .search_result table tr.company td ul li.right_box {
	float: right;
	margin-left: 30px;
}
.inner h3 {
  border-left: 5px solid #db433d;
  font-size: 16px;
  padding-left: 10px;
  color: #db433d;
  font-weight: bold;
  margin-bottom: 0;
}
table.details {
	width: 100%;
	margin-top: 0;
}
.search_result table tr.sum_price td {
	text-align: right;
	color: #db433d;
	border:none;
}
.search_result table tr td.right_none {
	border-right: none;
}
.search_result table tr td.left_none {
	border-left: none;
}
.list_section  {
	margin-top: 30px;
	border-bottom: 1px solid #dfdfdf;
}
.list_section dl {
	margin-bottom: 10px;}
.btn_area {
	text-align: center;
	margin: 30px 0;
}
#matter_section .special_input {
    border-right: medium none;
	width: 69px;
}
#matter_section ul li.last {
	margin-right: 0;
}
#matter_section .search_area dl dd.special_list {
	width: auto;
}
#matter_section .search_area dl dd.special_list select {
	float: left;
	width: 242px;
	margin-right: 5px;
}
#matter_section .search_area dl dd.special_list select.city {
	width: 130px;
}
#matter_section .state_list ul li.delete {
	float: right;
}
.search_result table tr td.order_info {
	text-align: left;
	color: #db433d;
	padding: 5px 10px;
	background: #ffeae9;
	border-left: 1px solid #ffeae9;
}
.search_result table tr td.order_info input {
	margin-right: 5px;
}
.search_result table tr td.order_num {
	background: #fff;
	border-left: 1px solid #fff;
}
.info_detail {
	background: #f6f4f5 none repeat scroll 0 0;
    border: 1px solid #dfdfdf;
    margin: 20px auto 0;
    padding: 10px 0 5px 35px;
    width: 990px;
}
.info_detail label {
	 width: 120px;
	 text-align: right;
}
.info_detail span {
	min-width: 190px;
	display: inline-block;
	color: #4798ed;
}
.info_detail .form-group label {
	float: left;}
.info_detail textarea {
	float: left;
	width: 750px;
	display: inline-block;
}
.search_result table tr td .btn {
	display: block;
    height: 26px;
    line-height: 26px;
    width: 25px;
	padding: 0;
}
.search_result table tr td .form-control {
	height: 26px;
}
.info_list dl {
	float: left;
}
.info_list dl .form-control {
	width: 350px;
}
.info_list dl dt {
	line-height: 28px;
}
.info_list dl dt span {
	color: #db433d;
}
.info_list .btn_add {
	margin-left: 20px;
	float: right;
}
.btn_area ul {
	display: inline-block;
}
.btn_area ul li {
	float: left;
	margin: 0 5px;
}