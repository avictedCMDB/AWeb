@charset "utf-8";
/* CSS Document */
#matter_section .search_area {
  background: #f6f4f5;
  padding: 25px 0 25px 15px;
  border: 1px solid #dfdfdf;
}
#matter_section .search_area dl {
	float: left;
	font-size: 12px;
	margin-bottom: 0;
	margin-right: 15px;
}
#matter_section .search_area dl dt {
	float: left;
	line-height: 25px;
	margin-right: 5px;
}
#matter_section .search_area dl dd {
	float: left;
}
#matter_section ul li {
	float: left;
	line-height: 25px;
	margin-right: 8px;
}
#matter_section .special_input {
	border-right: none;
}
#matter_section .form-control {
	height: 28px;
	width: 115px;
}
#matter_section .input-group-addon {
	height: 28px;
	padding: 7px 5px 5px 0px;
	border-left: none;
	background: #fff;
	cursor: pointer;
	display: inline-table;
}
#matter_section .btn {
	height: 28px;
	line-height: 28px;
	padding: 0 20px;
	border: none;
}
.red {
	background: #db433d;
	color: #fff;
}
.gray {
	background: #666666;
	color: #fff;
}
.search_result table {
	width: 950px;
	margin-top: 15px;
}
.search_result table tr th {
	background: #dfdfdf;
	border:1px solid #dfdfdf;
	border-right: 1px solid #fff;
	line-height: 32px;
	text-align: center;
	font-size: 12px;
}
.search_result table tr th.last {
	border-right: 1px solid #dfdfdf;
}
.search_result table tr td {
	border:1px solid #dfdfdf;
	padding: 10px;
	font-size: 12px;
	vertical-align: top;
}
.img_pro {
	border:1px solid #dfdfdf;
}
.price {
	color: #db433d;
}
#matter_section .search_result table tr td ul li {
	width: 135px;
	line-height: 1.42857;
	margin-right: 10px;
}
#matter_section .search_result table tr td ul li.last {
	margin-right: 0;
}
.page {
	text-align: right;
}
.pagination > li > a, .pagination > li  {
	color: #666666;
}
.pagination > li.active > a,
.pagination > li.active > a:hover,
.pagination > li > a:hover,
.pagination > li > a:focus {
  color: #fff;
  background-color: #db433d;
  border-color: #db433d;
}
