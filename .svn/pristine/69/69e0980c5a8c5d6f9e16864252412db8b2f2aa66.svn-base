@charset "utf-8";
/* CSS Document */
#matter_section .search_area {
  background: #f6f4f5;
  padding: 25px 0 15px 15px;
  border: 1px solid #dfdfdf;
}
#matter_section .search_area dl {
	float: left;
	font-size: 12px;
	margin-bottom: 10px;
	margin-right: 30px;
}
#matter_section .search_area dl.last {
	margin-right: 0;
}
#matter_section .search_area dl dt {
	float: left;
	line-height: 25px;
	margin-right: 5px;
	width: 75px;
	text-align: right;
}
#matter_section .search_area dl dd {
	float: left;
	width: 205px;
}
#matter_section ul li {
	float: left;
	line-height: 25px;
	margin-right: 8px;
}
#matter_section .special_input {
	border-right: none;
}
#matter_section .form-control {
	height: 28px;
	padding: 0;
}
#matter_section li .form-control {
	width: 69px;
}
#matter_section ul li.last {
	margin-right: 0;
}
#matter_section .input-group-addon {
	height: 28px;
	padding: 6px 5px 5px 0px;
	border-left: none;
	background: #fff;
	cursor: pointer;
	display: inline-table;
}
#matter_section .btn {
	height: 28px;
	line-height: 28px;
	padding: 0 20px;
	border: none;
}
.red {
	background: #db433d;
	color: #fff;
}
.gray {
	background: #666666;
	color: #fff;
}
.btn_list {
	text-align: right;
}
.btn_list ul {
	display: inline-block;
}
.search_result {
	padding-top: 15px;
}
.state_list {
	margin-bottom: 15px;
	overflow: hidden;
}
.state_list ul li a {
	display: block;
	width: 75px;
	text-align: center;
	border: 1px solid #dfdfdf;
}
.state_list ul li.active a,
.state_list ul li a:hover {
	background: #db433d;
	color: #fff;
}
#matter_section .state_list ul li.delete {
	float: right;
}

.section table {
	width: 950px;
}
.section table tr th {
	background: #dfdfdf;
	border:1px solid #dfdfdf;
	border-right: 1px solid #fff;
	line-height: 32px;
	text-align: center;
	font-size: 12px;
}
.section table tr th.last {
	border-right: 1px solid #dfdfdf;
}
.section table tr td {
	border:1px solid #dfdfdf;
	padding: 10px;
	font-size: 12px;
	vertical-align: middle;
	text-align: center;
}
.section table tr td a {
	word-break: break-all;
}
.section table tr td.right_none {
	border-right: none;
}
.section table tr td.left_none {
	border-left: none;
	text-align: left;
	color: #4798ed;
}
.section table tr.company td {
	border: none;
	font-weight: bold;
}
.section table tr td a {
	margin-right: 5px;
	color: #4798ed;
}
.section table tr td a:hover {
	text-decoration: underline;
}
.section table tr.company td span{
	font-weight: 100;
}
.img_pro {
	border:1px solid #dfdfdf;
	margin-bottom: 0;
}
.price {
	color: #db433d;
}
.section table tr td.price,
.section table tr td.num {
	vertical-align: middle;
}
#matter_section .section table tr.company td ul li.right_box {
	float: right;
	margin-left: 30px;
}
.section table tr td.left_align {
	text-align: left;
}
.section h3 {
	margin-bottom: 15px;
	border-left: 5px solid #db433d;
	font-size: 16px;
	padding-left: 10px;
	color: #db433d;
	font-weight: bold;
}
.section_info label {
	width: 200px;
	text-align: right;
	font-weight: 100;
	color: #0089d1;
}
.section_info span.special {
	width: 700px;
	width: 100%;
}
.detail_exchange table {
	width: 1170px;
}
.btn_close {
	margin: 20px 0;
	text-align: center;
}
.btn_close .btn {
	display: inline-block;
	height: 28px;
    line-height: 28px;
    padding: 0 20px;
	border: none;
}
form.special {
	text-align: center;
}
.special .form-group {
	margin: 0 30px;
}
.special .form-group input {
	width: 300px;
}
.special .form-group span.red {
	background: #fff;
	color: #db433d;
	margin-right: 5px;
}
.btn_close ul {
	display: inline-block;
}
.btn_close ul li {
	float: left;
	margin: 10px;
}
.last {
	margin-right: 0;
}
#matter_section .search_area dl dd.special_list {
	width: 530px;
}