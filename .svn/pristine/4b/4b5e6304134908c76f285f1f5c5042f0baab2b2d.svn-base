@charset "utf-8";
/* CSS Document */
#matter_section .search_area dl dt {
	width: 80px;
}
#matter_section .search_area dl dd {
	width: 202px;
}
.search_result table tr td.company {
	text-align: left;
}
.search_result table tr td.price {
	color: red;
	text-align: right;
}
/*添加发票*/
.add_details {
	width: 730px;
	margin: 25px auto;
	border: 1px solid #dfdfdf;
	background: #f6f5f5;
	padding: 30px 75px;
}
.add_details dl {
	margin-bottom: 10px;
}
.add_details dt {
	float: left;
	line-height: 28px;
	text-align: right;
	width: 95px;
}
.add_details dd {
	float: left;
	width: 460px;
	margin-left: 5px;
}


.add_details .form-control {
	height: 28px;
}
.add_details select {
	display: inline-block;
	width: 151px;
	padding: 0;
	}
.add_details span.red_remind {
	display: inline;
	color: #db433d;
	margin-right: 10px;
}
.btn_list {
	text-align: right;
}
.btn_list ul {
	display: inline-block;
}
.btn_list ul li {
	float: left;
	margin: 10px;
}

#matter_section .state_list {
	text-align: right;
}