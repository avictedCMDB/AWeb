
.search_cate {
	font-size: 16px;
}
body {
	font-size: 12px;
	color: #656565;
}
.inner {
	position: relative;
}
.font-color-red {
	color: #ff4200;
}

.font-16 {
	font-size: 16px;
}

.font-color-gray {
	color: #cacaca;
}

.breadcrumb {
	padding: 8px 0;
}

.breadcrumb li {
	float: left;
	padding-right: 10px;
	font-size: 12px;
}

.breadcrumb li span {
	padding-left: 10px;
}

#product_list {
	padding: 20px 20px 30px 20px;
	background-color: #fff;
}

#category {
	float: left;
	width: 180px;
	border: 1px solid #dfdfdd;
	border-top: 5px solid #db443d;
}

#category h2,
#category h3 {
	margin: 0;
	padding: 10px;
	font-size: 16px;
	font-weight: bold;
	background-color: #f1f1f1;
}
#category h3 {
	font-size: 14px;
	border-top: 1px dotted #dfdfdf;
	background: #f7f7f7;
	border-bottom: 1px dotted #dfdfdf;
	padding-left: 20px;
}

#category h2 img {
	float: right;
}

#category li {
	padding: 10px 0 10px 40px;
	background: url("../img/product-list/icon-arrow-right.gif") no-repeat 30px 15px;
	border-bottom: 1px dotted #dfdfdf;
	}
#category li li{
	background: none;
	padding: 10px 0 0px 5px;
	border: none;
}

#category li.none_dotted {
	border-bottom: none;
}



#brands_price {
	padding: 15px 0 25px;
	border: 1px solid #dfdfdd;
}



#brands_price .brands .sl_key {
	float: left;
	width: 36px;
	color: #ff4200;
	line-height: 36px;
	font-weight: bold;
	text-align: right;
}

#brands_price .brands .sl_value {
	float: right;
	width: 1100px;
	margin-right: 20px;
	background: url("../img/product-list/bg-brands.gif");
}

#brands_price .brands .sl_value li {
	padding-right: 5px;
	float: left;
	width: 85px;
	line-height: 36px;
	text-align: center;
}

#brands_price .brands .sl_value li a {
	color: #008ad3;
	padding: 5px 20px;
}
#brands_price .brands .sl_value li.active a,
#brands_price .brands .sl_value li a:hover {
	border: 1px solid #ff4200;
}
#brands_price .brands .sl_value .btn_more {
	z-index: 100;
	width: 1100px;
	text-align: center;
}

#sort_row {
	margin-top: 20px;
	padding: 5px 20px;
	border: 1px solid #dfdfdd;
	background-color: #f1f1f1;
}

#sort_row li a {
	margin-right: 8px;
	float: left;
	border: 1px solid #dcdcdc;
}

#sort_row li a.active {
	border: 1px solid #ff4200;
}

#sort_row li a:hover {
	display: block;
	border: 1px solid #ff4200;
}

#items {
	margin-top: 20px;
}

#items li {
	position: relative;
	float: left;
	margin: 0 10px 10px 0;
	padding: 10px 0;
	width: 226px;
	border: 1px solid #dfdfdd;
}

#items li .photo,
#items li .price,
#items li .describe,
#items li .btn_list {
	margin: 0 auto;
	width: 210px;
}
.btn_list .btn {
	padding: 2px 14px;
	font-size: 12px;
}
.btn_list .btn.red {
	color: #0091dc;
}
.btn_list .btn:hover {
	background: #fff;
	border: 1px solid #0091dc;
}
#items li .price {
	color: #ff4200;
	font-size: 18px;
	font-weight: bold;
}

#items .btn_list li {
	padding: 0;
	float: left;
	margin: 5px 5px 0 0;
	width: auto;
	border: 0;
}

#items .volume {
	position: absolute;
	top: 200px;
	right: 10px;
}

#items li.none {
	margin-right: 0;
}

#flip {
	margin-top: 20px;
	text-align: right;
}
#flip ul {
	display: inline-block;
}

#flip li {
	margin-right: 3px;
	padding: 5px 15px;
	float: left;
	border: 1px solid #ddd;
	border-radius: 2px;
	background-color: #f5f5f5;
}
#flip li.select {
	background-color: #fff;
	border: 0;
}
.write {
	float: right;
	background: #fff;
	border-radius: 4px;
	padding: 0 3px;
}
#table_list {
	margin-top: 20px;
}
#table_list table {
	width: 100%;
	table-layout: fixed;
}
#table_list tr th {
	background: #0091dc;
	color: #fff;
	padding: 8px 10px;
	text-align: center;
	font-size: 14px;
}
#table_list tr td {
	line-height: 20px;
	height: 20px;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
	padding: 8px 10px;
	text-align: center;
	border-bottom: 1px solid #dfdfdf;
}
#table_list tr td a {
	color: #0089d1;
}
#table_list tr td a:hover {
	text-decoration: underline;
}
#table_list tr td.price {
	color: #ff4200;
}
#table_list tr td.left_align {
	text-align: left;
}
#table_list tr td ul {
	display: inline-block;
}
#table_list tr td ul li {
	float: left;
	margin-left: 5px;
}
.table_btn {
	height: 20px;
	padding: 0 5px;
	background-color: #0091dc;
	border: none;
	color: #fff;
	font-size: 12px;
}
.table_btn i {
	font-size: 10px;
	margin-right: 2px;
}
#compare {
	display: none;
	position: absolute;
	bottom: 0px;
	left: 0px;
	width: 1206px;
	font-size: 12px;
	border: 2px solid #dc433d;
	z-index: 999;
	background-color: #fff;
}

#compare h2 {
	position: relative;
	margin: 0;
	padding: 10px;
	font-size: 16px;
	font-weight: bold;
	color: #fff;
	background-color: #dc433d;
}

#compare h2 span {
	position: absolute;
	top: 8px;
	right: 8px;
}

#compare_main {
	padding: 15px;
}

#compare_main li {
	padding: 5px 20px;
	float: left;
	border-right: 1px solid #dfdfdf;
}

#compare_main li p {
	float: left;
	margin: 0;
}

#compare_main li p.txt {
	padding-left: 10px;
	width: 140px;
	line-height: 56px;
}

#compare_main li dl {
	margin-bottom: 0;
	padding-top: 14px;
	width: 140px;
	float: right;
}
#compare_main .compare_btn {
	padding: 12px 0 0 70px;
	text-align: center;
	border: none;
}

#compare_main .compare_btn a {
	color: #018ad0;
}
/*热门推荐*/
#hotsale {
	background: #f1f1f1;
	position: relative;
	padding:0 20px;
}
#hotsale .title_hot {
	background: url(../img/product-list/list.icons.other.png) left top no-repeat;
	font-size: 14px;
    height: 80px;
    line-height: 18px;
    padding: 20px 10px 0 5px;
    text-align: center;
    width: 45px;
	color: #fff;
	float: left;
	margin-right: 5px;
}
#hotsale .hot_list {
	float: left;
	padding: 20px 0;
}
#hotsale .hot_list dl {
	float: left;
	margin-bottom: 0;
	width: 350px;
	margin-left: 10px;
	background: url(../img/product-list/bg-hot.png) right bottom no-repeat #fff;
	padding: 10px;
}
#hotsale .hot_list dl dt {
	width: 98px;
	border: 1px solid #dfdfdf;
}
#hotsale .hot_list dl dd {
	margin-left: 118px;
}
#hotsale .hot_list dl .price{
	color: #ff4200;
	font-size: 18px;
	font-weight: bold;
	margin-top: 10px;
	margin-bottom: 10px;
}
#menu_cate {
	margin: 20px 0;
}
#menu_cate ul li {
	float: left;
	font-size: 16px;
	z-index: 2;
	position: relative;
	line-height: 28px;
}
#menu_cate ul li a.cate_title {
	font-weight: bold;
	padding-top: 2px;
}
#menu_cate ul {
	z-index: 1;
	position: relative;
}
#menu_cate ul li i {
	margin: 0 10px;
}
#menu_cate ul li a.cate_list {
	border: 1px solid #dfdfdf;
	padding:0px 12px;
	font-size: 12px;
	background: #fff;
	line-height: 26px;
	
	display: inline-block;
}
#menu_cate ul li a.cate_list:hover {
	border: 1px solid #db4340;
	border-bottom:1px solid #fff;
}
#menu_cate .details ul {
	position: absolute;
	background: #fff;
	width: 300px;
	border: 1px solid #db4340;
	top: 28px;
	z-index: -1;
	padding: 10px;
}
#menu_cate .details ul li {
	font-size: 12px;
	margin-right: 10px;
	width: 55px;
}
#compare h2 {
	background:#018ad0;
}
#compare {
	border: 2px solid #018ad0;
}