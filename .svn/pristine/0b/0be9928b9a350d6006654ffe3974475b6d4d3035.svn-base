            display: flex;
            .item{
                flex: 1;
                height: 36px;
                text-align: center;
                a{
                    display: inline-block;
                    height: 36px;
                    line-height: 36px;
                    width: 90%;
                    color: #fff;
                    text-decoration: none;
                    padding-right: 10px;
                    background-image: url(../image/type_bg.jpg);
                    background-size: 110% auto;
                    background-position-x: 95%;
                }
            }
        }
        .tip{
            width: 100%;
        }
    }
    .pro-brand{
        .hd{
            width: 100%;
            display: block;
        }
        .sup-list{
            background-color: #fff;
            padding: 0 10px 10px 10px;
            .row{
                padding-top: 10px;
                position: relative;
                display: flex;
                .item{
                    flex: 1;
                    position: relative;
                    text-align: center;
                    img{
                        width: 90%;
                        border: 1px solid #eee;
                    }
                }
            }
        }
    }
    .pro-latest {
        .hd {
            width: 100%;
            display: block;
        }
        .sup-list{
            background-color: #fff;
            padding: 10px;
            .sup{
                padding: 8px 100px 8px 0;
                font-size: 14px;
                color: #333333;
                position: relative;
                overflow: hidden;
                text-overflow: ellipsis;
                white-space: nowrap;
                border-bottom: 1px solid #eee;
                &:before{
                    content: ' ';
                    background-color: @base-color;
                    display: inline-block;
                    width: 8px;
                    height: 8px;
                    margin-bottom: 1px;
                    margin-right: 5px;
                    border-radius: 4px;
                }
                span{
                    position: absolute;
                    right: 0;
                    color: #666;
                }
            }
        }
    }
    .high-sup {
        .hd {
            width: 100%;
            display: block;
        }
        .sup-list {
            background-color: #fff;
            padding: 0 10px 5px 10px;
            .item{
                padding: 5px 5px 5px 85px;
                height: 80px;
                position: relative;
                border-bottom: 1px solid #eee;
                &:last-of-type{
                    border-bottom: none;
                }
                .i{
                    width: 100%;
                    height: 100%;
                    p{
                        overflow: hidden;
                        text-overflow: ellipsis;
                        white-space: nowrap;
                        &.name{
                            padding-bottom: 10px;
                        }
                        &.type,&.info{
                            font-size: 14px;
                        }
                    }
                }
                img{
                    width: 70px;
                    height: 70px;
                    position: absolute;
                    left: 5px;
                    top: 5px;
                }
            }
        }

    }
    .select-sup{
        display: block;
        width: 100%;
    }
}

.mask-bg{
    background-color: rgba(0,0,0,.3);
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 999;
}

.border-bottom{
    position: relative;
    &:after{
        z-index: 2;
        content: ' ';
        width: 100%;
        height: 1px;
        background-color: #ccc;
        bottom: -1px;
        left: 0;
        position: absolute;
        .scale-border-y;
    }
}
html,body{
    width: 100%;
    height: 100%;
}
