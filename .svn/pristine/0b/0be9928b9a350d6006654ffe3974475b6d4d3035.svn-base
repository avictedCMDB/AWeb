                word-break: break-all;
                position: relative;
                .p{
                    position: absolute;
                    bottom: 5px;
                    font-size: 16px;
                    color: #f00;
                }
            }
            .op{
                flex: 1;
                text-align: center;
                border-left: 1px solid #ddd;
                line-height: 90px;
                font-size: 14px;
            }
        }
    }
}
.cate{
    background-color: #eee;
    overflow: hidden;
    .right{
