/* RESET START */
html, body, div, span, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
abbr, address, cite, code,
del, dfn, em, img, ins, kbd, q, samp,
small, strong, sub, sup, var,
b, i,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, figcaption, figure,
footer, header, hgroup, menu, nav, section, summary,
time, mark, audio, video {
    margin: 0;
    padding: 0;
    border: 0;
    outline: 0;
    font-size: 100%;
    vertical-align: baseline;
    background: transparent;
    list-style: none;
}
*{
    font-family:'Tahoma','Arial','Roboto','Droid Sans','Helvetica Neue','Droid Sans Fallback','Heiti SC','Hiragino Sans GB',Simsun,sans-self!important;
    -webkit-tap-highlight-color:rgba(0,0,0,0)!important;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    border: 0;
    outline: 0;
}
/* RESET END */
@input-color: #666;
@font-color: #333;
@base-color: #067cca;
/* INDEX START */
body{
    .footer{
        display: none;
    }
}
body.index{
    background-color: #eee;
    .search-wrapper{
        position: relative;
        .search{
            position: absolute;
            width: 70%;
            height: 34px;
            border: 1px solid #666;
            background-color: #fff;
            box-shadow: 0 0 1px 1px #666;
            top: 50%;
            left: 15%;
            display: table;
            border-collapse: separate;
            border-radius: 5px;
        }
        .iconfont{
            display: table-cell;
            width: 1%;
            font-size: 18px;
            padding: 7px 8px;
            color: @input-color;
        }
        input{
            display: table-cell;
            width: 100%;
            font-size: 15px;
            border-bottom-right-radius: 5px;
            border-top-right-radius: 5px;
            color: @input-color;
        }
        img{
            width: 100%;
            display: block;
        }
    }
    .nav-wrapper{
        padding: 8px 8%;
        height: 90px;
        background-color: #fff;
        .nav{
            display: flex;
            position: relative;
            height: 100%;
            li{
                flex: 1;
                height: 100%;
                text-align: center;
                img{
                    width: 50px;
                }
                p{
                    color: @input-color;
                }
            }
        }
    }
    .hot{
        background: url("../image/block-hot.jpg") repeat-y;
        background-size: 100% auto;
        padding: 8px 6px;
        p{
            color: #fff;
            text-align: center;
            padding-bottom: 8px;
            a{
                padding: 0 4px;
                font-size: 18px;
            }
        }
        .item{
            width: 50%;
            padding: 2px;
            line-height: 0;
            float: left;
            img{
                width: 100%;
            }
        }
    }
    .cate{
        .hd{
            background: url("../image/cate-bg.jpg") repeat;
            background-size: 24% auto;
            height: 38px;
            img{
                width: 38px;
                vertical-align: middle;
                background-color: inherit;
            }
            .left{
                padding: 3px;
            }
            .title{
                vertical-align: middle;
                color: #fff;
                font-size: 20px;
                font-weight: bold;
                line-height: 36px;
                &:after{
                    content: attr(data-desc);
                    font-size: 12px;
                    font-weight: 300;
                    font-style: italic;
                    padding-left: 5px;
                }
            }
            .right{
                float: right;
                padding: 10px;
            }
        }
        .prod{
            padding: 0px 2px 6px 2px;
            background-color: #fff;
            .row{
                display: flex;
                width: 100%;
                border-top: 1px solid #eee;
                &:nth-of-type(1){
                    border-top: 0;
                }
                .item{
                    flex: 1;
                    padding: 4px;
                    img{
                        width: 42%;
                        max-width: 80px;
                        min-height: 80px;
                        vertical-align: middle;
                    }
                    div{
                        display: inline-block;
                        vertical-align: middle;
                        width: 55%;
                        text-align: center;
                        p{
                        }
                        .name{
                            font-size: 14px;
                        }
                        .desc{
                            font-size: 8px;
                            padding-bottom: 8px;
                            word-break: break-all;
                        }
                        .buy{
                            color: #fff;
                            font-size: 12px;
                            padding: 4px 14px;
                            background-color: @base-color;
                        }
                    }
                    &:nth-of-type(even){
                        border-left: 1px solid #eee;
                    }

                }

            }
        }
    }
}
body.include-footer{
    padding-bottom: 60px;
    .footer{
        z-index: 5;
        position: fixed;
        bottom: 0;
        width: 100%;
        height: 50px;
        background-color: #fff;
        padding: 4px 14px 14px 2px;
        display: flex;
        box-shadow: 0 0 10px 0 #999;
        .item{
            flex: 1;
            text-align: center;
            text-decoration: none;
            span{
                display: block;
                font-size: 12px;
                color: #555;
            }
            .iconfont{
                font-size: 26px;
            }
            &.active .iconfont{
                color: @base-color;
            }
        }
    }
}
body.search{
    .header{
        z-index: 5;
        background: url("../image/cate-bg.jpg") repeat;
        background-size: 24% auto;
        padding: 6px 8px 6px 2px;
        display: table;
        border-collapse: separate;
        width: 100%;
        height: 46px;
        top: 0;
        *{
            display: table-cell;
            vertical-align: middle;
        }
        .back{
            color: #fff;
            font-size: 20px;
            width: 1%;
            padding: 2px 4px 4px 2px;
        }
        .input{
            width: 100%;
            background-color: #fff;
            border-radius: 5px;
            display: inline-table;
            .iconfont{
                font-size: 20px;
                color: @input-color;
                padding: 3px 6px;
                display: table-cell;
                width: 1%;
            }
            input{
                display: table-cell;
                color: @input-color;
                width: 100%;
                padding: 4px 0;
                font-size: 16px;
                border-radius: 5px;
                height: 34px;
            }
        }
    }
    .order{
        z-index: 5;
        width: 100%;
        height: 38px;
        display: flex;
        top: 46px;
        background-color: #fff;
        li{
            flex: 1;
            text-align: center;
            line-height: 38px;
            font-size: 14px;
            position: relative;
            &.full-right-border{
                &:before{
                    height: 100%;
                    top: 0;
                }
            }
            &:before{
                content: ' ';
                left: 0;
                top: 8px;
                position: absolute;
                background-color: #ddd;
                width: 1px;
                height: 22px;
                .scale-border-x;
                .transition-ease-300;
            }
            &:after{
                content: ' ';
                left: 0;
                bottom: -1px;
                position: absolute;
                height: 1px;
                background-color: #ddd;
                width: 100%;
                .scale-border-y;
            }
            &:nth-of-type(1):before{
                display: none;
            }
            &.active{
                color: @base-color;
                &:after{
                    width: 0;
                }
                span:after{
                    color: @base-color;
                    content: '\e728';
                }
            }
            span:after{
                font-family: iconfont;
                content: '\e729';
                font-size: 14px;
                color: #999;
                padding-left: 2px;
            }

        }
    }
    .order-mask{
        position: fixed;
        top: 84px;
        bottom: 0;
        width: 100%;
        background-color: rgba(0,0,0,.4);
        z-index: 99;
    }
    .cnd{
        display: none;
        z-index: 100;
        width: 100%;
        background-color: #fff;
        position: fixed;
        top: 84px;
        overflow: hidden;
        .cnd-list{
            position: relative;
            padding: 6px 6px 0 6px;
            .item{
                display: inline-block;
                width: 32.33%;
                text-align: center;
                padding: 6px;
                background-color: #eee;
                margin-bottom: 8px;
                font-size: 14px;
                color: @font-color;
                border-radius: 3px;
                &.active{
                    background-color: @base-color;
                    color: #fff;
                }
            }
            &.c1 .item{
                width: 49%;
            }
        }

        .button-group{
            position: relative;
            display: flex;
            .btn{
                display: inline-block;
                flex: 1;
                text-align: center;
                padding: 8px 0;
                font-size: 14px;
                position: relative;
                &:before{
                    content: ' ';
                    left: 0;
                    top: 0;
                    position: absolute;
                    height: 100%;
                    background-color: #ddd;
                    width: 1px;
                    .scale-border-x;
                }
                &:nth-of-type(1):before{
                    display: none;
                }
                &.btn-primary{
                    color: @base-color;
                }
                &.btn-disable{
                    color: @input-color;
                }
            }
            &:before{
                content: ' ';
                top: 0;
                position: absolute;
                height: 1px;
                background-color: #ddd;
                width: 100%;
                .scale-border-y;
            }
        }
    }
    .prod-list{
        .prod-item{
            position: relative;
            height: 90px;
            img{
                height: 90px;
                padding: 5px;
                float: left;
            }
            div{
                .n{
                    padding: 10px 10px 0 10px;
                    word-break: break-all;
                    font-size: 14px;
                    height: 44px;
                    overflow: hidden;
                    -webkit-line-clamp: 2;
                    line-clamp: 2;
                    display: -webkit-box;
                    -webkit-box-orient: vertical;
                    color: @font-color;
                }
                .p{
                    display: -webkit-box;
                    padding: 0 10px;
                    color: #f00;
                }
                .c{
                    font-size: 14px;
                    display: -webkit-box;
                    padding: 0 10px;
                    color: @input-color;
                }
            }

            &:after{
                content: ' ';
                left: 0;
                bottom: -1px;
                position: absolute;
                height: 1px;
                background-color: #ddd;
                width: 100%;
                .scale-border-y;
            }
        }
    }
    #search.fix{
        position: fixed;
    }
}
img.big{
    display: block;
    line-height: 0;
    width: 100%;
}
.margin-top-10{
    margin-top: 10px;
}
.clear{
    clear: both;
}
.scale-border-x{
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleX(0.5);
    transform: scaleX(0.5);
}
.scale-border-y{
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}
.transition-ease-300{
    -webkit-transition: all ease .3s;
    transition: all ease .3s;
}
.fix{
    overflow: hidden;
}
.show{
    display: block!important;
}
.hide{
    display: none!important;
}
/* INDEX END */
