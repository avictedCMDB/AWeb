function removeSupGoods(index){
	alert(index);
	if(index==1){
		$("#supOneID").val("");
		$("#supGoodsOneID").val("");
	}else if(index==2){
		$("#supTwoID").val("");
		$("#supGoodsTwoID").val("");
	}else if(index==3){
		$("#supThreeID").val("");
		$("#supGoodsThreeID").val("");
	}else if(index==4){
		$("#supFourID").val("");
		$("#supGoodsFourID").val("");
	}
	document.getElementById("commitForm").action="/AWeb/management/compare/detail";
	document.getElementById("commitForm").submit();
}

