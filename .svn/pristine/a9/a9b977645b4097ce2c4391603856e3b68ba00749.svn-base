  <!--#header--></div>
  
  
  <div id="contents">
    <div class="inner details">
      <h2>换货管理</h2>
      <div class="section">
        <h3>换货单基本信息</h3>
        <div class="section_info">
          <label>发货单编号：</label>
          <span></span>
          <label>订单编号：</label>
          <span><a href="#">D100003110000</a></span>
          <label>换货单编号：</label>
          <span>H100001110000</span>
          <label>换货总数量：</label>
          <span>3</span>
          <label>供应商名称： 	</label>
          <span>晨光科力普</span>
        </div>
        <div class="section_info">
          <label>备注：</label>
          <span>备注备注备注备注</span>
        </div>
        <div class="section_info">
          <label>附件：</label>
          <span></span>
        </div>
        <div class="section_info">
          <label>供应商联系地址：</label>
          <span></span>
        </div>
      </div>
      <div class="section detail_exchange">
        <h3>换货单明细信息</h3>
        <table border="0">
          <tr>
            <th width="40">序号</th>
            <th colspan="2">商品信息</th>	
            <th width="110">单价(元)</th>
            <th width="110">换货数量</th>
            <th width="110">退换金额</th>
            <th width="200">换货原因</th>
            <th width="110" class="last">操作</th>
          </tr>
          <tr>
            <td>1</td>
            <td class="right_none" width="57"><p class="img_pro"><a href="#"><img src="img/management/img-order.jpg" /></a></p></td>
            <td class="left_none"><a href="#">晨光 M&G 优事贴自粘便条纸AS32A10110 3"*2" 76mm*51mm 100页/本（黄色）40本/盒</a></td>
            <td class="price">1.54</td>
            <td>2</td>
            <td>3.08</td>
            <td></td>
            <td><a href="#">查看</a></td>
          </tr>
        </table>
      </div>
      <div class="section">
        <h3>物流信息</h3>
        <form class="form-inline special" role="form">
          <div class="form-group">
            <label><span class="red">*</span>快递公司：</label>
            <input type="text" class="form-control">
          </div>
          <div class="form-group">
            <label><span class="red">*</span>快递编号：</label>
            <input type="text" class="form-control">
          </div>
        </form>
      </div>
      <div class="btn_close">
        <ul class="clearfix">
          <li><button type="button" class="btn btn-default red">发货</button></li>
          <li><button type="button" class="btn btn-default gray">关闭</button></li>
        </ul>
      </div>
      
      
