@charset "utf-8";
/* CSS Document */
.link_cate ul {
	padding: 0;
	margin: 0;
}
#matter_section .state_list ul li {
	margin-right: 2px;
}
.state_list ul li a {
	width: 30px;
}
/*添加超市*/

.add_details {
	width: 730px;
	margin: 0 auto;
	border: 1px solid #dfdfdf;
	background: #f6f5f5;
	margin-top: 25px;
	padding: 30px 75px;
}
.add_details dl {
	margin-bottom: 10px;
}
.add_details dt {
	float: left;
	line-height: 28px;
	text-align: right;
	width: 110px;
}
.add_details dd {
	float: left;
	width: 460px;
	margin-left: 5px;
}
.add_details dd p {
	margin-top: 10px;
	margin-bottom: 0;
	
}

.add_details .form-control {
	height: 28px;
}
.add_details select {
	display: inline-block;
	width: 151px;
	padding: 0;
	}
.add_details span.red_remind {
	display: inline;
	color: #db433d;
	margin-right: 10px;
}
.btn_middle {
	text-align: center;
	margin-top: 30px;
}
#contents .details{
	padding-bottom: 60px;
}
.check dd {
	line-height: 28px;
}
.info_list {
	margin-left: 10px;
}
.add_details textarea.form-control {
	height: auto;
}

.add_details ul li {
	float: left;
	margin-right: 5px;
	line-height: 28px;
}
.btn_middle ul {
	display: inline-block;
}

/*超市信息查看*/
.section_info h3 {
	margin-bottom: 15px;
  border-left: 5px solid #0091dc;
  font-size: 16px;
  padding-left: 10px;
  color: #0091dc;
  font-weight: bold;
}
.section_inner {
	 width: 950px;
	 margin: 0 auto;
}
.section_inner .search_area {
	background: #f6f4f5;
	padding: 10px;
}
.section_inner .search_area dl dt {
	line-height: 28px;
}
.section_inner .search_area dl {
	margin-bottom: 0;
}
.section_inner .search_area dl dd {
	width: 600px;
}
/*销售范围及优惠率查看*/
.sale_area {
	width: 900px;
	margin: 0 auto;
}
.sale_area h2 {
	text-align: center;
	background: #0091dc;
	color: #fff;
	font-size: 16px;
	padding: 5px 0;
	margin-top: 0;
	margin-bottom: 0;
}
.sale_area h3 {
    border-left: 5px solid #0091dc;
    font-size: 14px;
    padding-left: 10px;
    color: #0091dc;
    font-weight: bold;
	margin: 0 0 10px 0;
}
.sale_area label {
	margin-left: 120px;
}
.sale_area .section_inner {
	width: 860px;
	margin: 0 auto;
}
.sale_area table {
	width: 860px;
	margin-top: 0;
}
.sale_area ul li {
	float: left;
	line-height: 25px;
    margin-right: 9px;
}

