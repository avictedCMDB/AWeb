@charset "utf-8";
/* CSS Document */
#header {
    margin-bottom: 30px;
    padding-top: 30px;
}
#header .logo {
	margin-top: 10px;
}
#matter_section h2 {
	font-size: 16px;
	color: #0091dc;
	border-bottom: 1px solid #dfdfdf;
	text-align: center;
	padding-bottom: 5px;
	font-weight: bold;
	margin-bottom: 15px;
}
#matter_section .tab {
	margin-bottom: 20px;
}
#matter_section .tab ul li {
	margin-right: 10px;
}
#matter_section .tab ul li a {
	border: none;
	border: 1px solid #dfdfdf;
	border-radius: 0;
}
#matter_section .tab .nav-tabs {
	border-bottom: none;
}
#matter_section .tab .nav > li > a {
	padding: 5px 15px;
}
#matter_section .tab .nav-tabs > li.active > a, #matter_section .tab .nav-tabs > li.active > a:hover, #matter_section .tab .nav-tabs > li.active > a:focus {
	border: none;
	background: none;
	border: 1px solid #ff4200;
	color: #ff4200;
}
#matter_section .tab .nav > li > a:hover,
#matter_section .tab .nav > li > a:focus {
	text-decoration: none;
	background-color: #fff;
}
#matter_section .border_right {
	border-bottom-right-radius: 4px;
	border-top-right-radius:4px;
	border-left: none;
}
#matter_section .border_left {
	border-bottom-left-radius: 4px;
	border-top-left-radius:4px;
}
#matter_section span.text {
	padding-right: 5px;
}
.btn_list {
	text-align: right;
	padding-right: 10px;
}