<link href="${pageContext.request.contextPath}/static/css/management_address.css" rel="stylesheet">
<!-- Fontello CSS -->
<link href="${pageContext.request.contextPath}/static/fonts/fontello/css/fontello.css" rel="stylesheet">
<script type="text/javascript" src="${pageContext.request.contextPath}/static/js/jquery.js"></script>
<script type="text/javascript" src="${pageContext.request.contextPath}/static/js/rollover.js"></script>
<script type="text/javascript" src="${pageContext.request.contextPath}/static/js/managementAddress.js"></script>
</head>
<body>
<div class="page-wrapper">
	<div id="side_navi">
   		<dl>
   			<dt class="tlt_merchandise">商品比对管理<span class="fa fa-caret-square-o-down"></span></dt>
   			<dd>
   				<ul>
   					<li <c:if test="${param.menu == 'compare'}">class="active"</c:if>><a href="${pageContext.request.contextPath}/management/compare"><span class="fa fa-caret-right"></span>商品比对管理</a></li>
   				</ul>
   			</dd>
   			<dt class="tlt_information">信息管理<span class="fa fa-caret-square-o-down"></span></dt>
   			<dd>
   				<ul>
   					<li <c:if test="${param.menu == 'address'}">class="active"</c:if>><a href="${pageContext.request.contextPath}/management/address"><span class="fa fa-caret-right"></span>地址管理</a></li>
   					<li <c:if test="${param.menu == 'invoice'}">class="active"</c:if>><a href="${pageContext.request.contextPath}/management/invoice"><span class="fa fa-caret-right"></span>发票管理</a></li>
   				</ul>
   			</dd>
   			<dt class="tlt_transaction">交易管理<span class="fa fa-caret-square-o-down"></span></dt>
   			<dd>
   				<ul>
   					<li><a href="#"><span class="fa fa-caret-right"></span>购物车管理</a></li>
