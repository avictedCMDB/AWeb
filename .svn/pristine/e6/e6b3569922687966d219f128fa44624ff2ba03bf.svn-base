/* RESET START */
html,
body,
div,
span,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
abbr,
address,
cite,
code,
del,
dfn,
em,
img,
ins,
kbd,
q,
samp,
small,
strong,
sub,
sup,
var,
b,
i,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section,
summary,
time,
mark,
audio,
video {
  margin: 0;
  padding: 0;
  border: 0;
  outline: 0;
  font-size: 100%;
  vertical-align: baseline;
  background: transparent;
  list-style: none;
}
* {
  font-family: 'Tahoma', 'Arial', 'Roboto', 'Droid Sans', 'Helvetica Neue', 'Droid Sans Fallback', 'Heiti SC', 'Hiragino Sans GB', Simsun, sans-self !important;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0) !important;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  border: 0;
  outline: 0;
}
/* RESET END */
/* INDEX START */
body .footer {
  display: none;
}
body.index {
  background-color: #eee;
}
body.index .search-wrapper {
  position: relative;
}
body.index .search-wrapper .search {
  position: absolute;
  width: 70%;
  height: 34px;
  border: 1px solid #666;
  background-color: #fff;
  box-shadow: 0 0 1px 1px #666;
  top: 50%;
  left: 15%;
  display: table;
  border-collapse: separate;
  border-radius: 5px;
}
body.index .search-wrapper .iconfont {
  display: table-cell;
  width: 1%;
  font-size: 18px;
  padding: 7px 8px;
  color: #666;
}
body.index .search-wrapper input {
  display: table-cell;
  width: 100%;
  font-size: 15px;
  border-bottom-right-radius: 5px;
  border-top-right-radius: 5px;
  color: #666;
}
body.index .search-wrapper img {
  width: 100%;
  display: block;
}
body.index .nav-wrapper {
  padding: 8px 8%;
  height: 90px;
  background-color: #fff;
}
body.index .nav-wrapper .nav {
  display: flex;
  position: relative;
  height: 100%;
}
body.index .nav-wrapper .nav li {
  flex: 1;
  height: 100%;
  text-align: center;
}
body.index .nav-wrapper .nav li img {
  width: 50px;
}
body.index .nav-wrapper .nav li p {
  color: #666;
}
body.index .hot {
  background: url("../image/block-hot.jpg") repeat-y;
  background-size: 100% auto;
  padding: 8px 6px;
}
body.index .hot p {
  color: #fff;
  text-align: center;
  padding-bottom: 8px;
}
body.index .hot p a {
  padding: 0 4px;
  font-size: 18px;
}
body.index .hot .item {
  width: 50%;
  padding: 2px;
  line-height: 0;
  float: left;
}
body.index .hot .item img {
  width: 100%;
}
body.index .cate .hd {
  background: url("../image/cate-bg.jpg") repeat;
  background-size: 24% auto;
  height: 38px;
}
body.index .cate .hd img {
  width: 38px;
  vertical-align: middle;
  background-color: inherit;
}
body.index .cate .hd .left {
  padding: 3px;
}
body.index .cate .hd .title {
  vertical-align: middle;
  color: #fff;
  font-size: 20px;
  font-weight: bold;
  line-height: 36px;
}
body.index .cate .hd .title:after {
  content: attr(data-desc);
  font-size: 12px;
  font-weight: 300;
  font-style: italic;
  padding-left: 5px;
}
body.index .cate .hd .right {
  float: right;
  padding: 10px;
}
body.index .cate .prod {
  padding: 0px 2px 6px 2px;
  background-color: #fff;
}
body.index .cate .prod .row {
  display: flex;
  width: 100%;
  border-top: 1px solid #eee;
}
body.index .cate .prod .row:nth-of-type(1) {
  border-top: 0;
}
body.index .cate .prod .row .item {
  flex: 1;
  padding: 4px;
}
body.index .cate .prod .row .item img {
  width: 42%;
  max-width: 80px;
  min-height: 80px;
  vertical-align: middle;
}
body.index .cate .prod .row .item div {
  display: inline-block;
  vertical-align: middle;
  width: 55%;
  text-align: center;
}
body.index .cate .prod .row .item div .name {
  font-size: 14px;
}
body.index .cate .prod .row .item div .desc {
  font-size: 8px;
  padding-bottom: 8px;
  word-break: break-all;
}
body.index .cate .prod .row .item div .buy {
  color: #fff;
  font-size: 12px;
  padding: 4px 14px;
  background-color: #067cca;
}
body.index .cate .prod .row .item:nth-of-type(even) {
  border-left: 1px solid #eee;
}
body.include-footer {
  padding-bottom: 60px;
}
body.include-footer .footer {
  z-index: 5;
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 50px;
  background-color: #fff;
  padding: 4px 14px 14px 2px;
  display: flex;
  box-shadow: 0 0 10px 0 #999;
}
body.include-footer .footer .item {
  flex: 1;
  text-align: center;
  text-decoration: none;
}
body.include-footer .footer .item span {
  display: block;
  font-size: 12px;
  color: #555;
}
body.include-footer .footer .item .iconfont {
  font-size: 26px;
}
body.include-footer .footer .item.active .iconfont {
  color: #067cca;
}
body.search .header {
  z-index: 5;
  background: url("../image/cate-bg.jpg") repeat;
  background-size: 24% auto;
  padding: 6px 8px 6px 2px;
  display: table;
  border-collapse: separate;
  width: 100%;
  height: 46px;
  top: 0;
}
body.search .header * {
  display: table-cell;
  vertical-align: middle;
}
body.search .header .back {
  color: #fff;
  font-size: 20px;
  width: 1%;
  padding: 2px 4px 4px 2px;
}
body.search .header .input {
  width: 100%;
  background-color: #fff;
  border-radius: 5px;
  display: inline-table;
}
body.search .header .input .iconfont {
  font-size: 20px;
  color: #666;
  padding: 3px 6px;
  display: table-cell;
  width: 1%;
}
body.search .header .input input {
  display: table-cell;
  color: #666;
  width: 100%;
  padding: 4px 0;
  font-size: 16px;
  border-radius: 5px;
  height: 34px;
}
body.search .order {
  z-index: 5;
  width: 100%;
  height: 38px;
  display: flex;
  top: 46px;
  background-color: #fff;
}
body.search .order li {
  flex: 1;
  text-align: center;
  line-height: 38px;
  font-size: 14px;
  position: relative;
}
body.search .order li.full-right-border:before {
  height: 100%;
  top: 0;
}
body.search .order li:before {
  content: ' ';
  left: 0;
  top: 8px;
  position: absolute;
  background-color: #ddd;
  width: 1px;
  height: 22px;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
  -webkit-transition: all ease .3s;
  transition: all ease .3s;
}
body.search .order li:after {
  content: ' ';
  left: 0;
  bottom: 0;
  position: absolute;
  height: 1px;
  background-color: #ddd;
  width: 100%;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
body.search .order li:nth-of-type(1):before {
  display: none;
}
body.search .order li.active {
  color: #067cca;
}
body.search .order li.active:after {
  width: 0;
}
body.search .order li.active span:after {
  color: #067cca;
  content: '\e728';
}
body.search .order li span:after {
  font-family: iconfont;
  content: '\e729';
  font-size: 14px;
  color: #999;
  padding-left: 2px;
}
body.search .order-mask {
  position: fixed;
  top: 84px;
  bottom: 0;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 99;
}
body.search .cnd {
  display: none;
  z-index: 100;
  width: 100%;
  background-color: #fff;
  position: fixed;
  top: 84px;
  overflow: hidden;
}
body.search .cnd .cnd-list {
  position: relative;
  padding: 6px 6px 0 6px;
}
body.search .cnd .cnd-list .item {
  display: inline-block;
  width: 32.33%;
  text-align: center;
  padding: 6px;
  background-color: #eee;
  margin-bottom: 8px;
  font-size: 14px;
  color: #333;
  border-radius: 3px;
}
body.search .cnd .cnd-list .item.active {
  background-color: #067cca;
  color: #fff;
}
body.search .cnd .cnd-list.c1 .item {
  width: 49%;
}
body.search .cnd .button-group {
  position: relative;
  display: flex;
}
body.search .cnd .button-group .btn {
  display: inline-block;
  flex: 1;
  text-align: center;
  padding: 8px 0;
  font-size: 14px;
  position: relative;
}
body.search .cnd .button-group .btn:before {
  content: ' ';
  left: 0;
  top: 0;
  position: absolute;
  height: 100%;
  background-color: #ddd;
  width: 1px;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
}
body.search .cnd .button-group .btn:nth-of-type(1):before {
  display: none;
}
body.search .cnd .button-group .btn.btn-primary {
  color: #067cca;
}
body.search .cnd .button-group .btn.btn-disable {
  color: #666;
}
body.search .cnd .button-group:before {
  content: ' ';
  top: 0;
  position: absolute;
  height: 1px;
  background-color: #ddd;
  width: 100%;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
body.search .prod-list .prod-item {
  position: relative;
  height: 90px;
}
body.search .prod-list .prod-item img {
  height: 90px;
  padding: 5px;
  float: left;
}
body.search .prod-list .prod-item div .n {
  padding: 10px 10px 0 10px;
  word-break: break-all;
  font-size: 14px;
  height: 44px;
  overflow: hidden;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  color: #333;
}
body.search .prod-list .prod-item div .p {
  display: -webkit-box;
  padding: 0 10px;
  color: #f00;
}
body.search .prod-list .prod-item div .c {
  font-size: 14px;
  display: -webkit-box;
  padding: 0 10px;
  color: #666;
}
body.search .prod-list .prod-item:after {
  content: ' ';
  left: 0;
  bottom: 0;
  position: absolute;
  height: 1px;
  background-color: #ddd;
  width: 100%;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
body.search #search.fix {
  position: fixed;
}
img.big {
  display: block;
  line-height: 0;
  width: 100%;
}
.margin-top-10 {
  margin-top: 10px;
}
.clear {
  clear: both;
}
.scale-border-x {
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
}
.scale-border-y {
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.transition-ease-300 {
  -webkit-transition: all ease .3s;
  transition: all ease .3s;
}
.fix {
  overflow: hidden;
}
.show {
  display: block!important;
}
.hide {
  display: none!important;
}
/* INDEX END */
body.my {
  background-color: #eee;
}
body.my .user {
  height: 140px;
  background: url("../image/my-head-bg.png");
  background-size: 100% 100%;
  position: relative;
}
body.my .user .head {
  display: block;
  width: 80px;
  height: 80px;
  background-color: #fff;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -40px;
  margin-top: -55px;
  border-radius: 40px;
  text-align: center;
}
body.my .user .name {
  text-align: center;
  position: absolute;
  color: #fff;
  width: 100%;
  bottom: 15px;
}
body.my .tab {
  height: 70px;
  background-color: #fff;
  display: flex;
}
body.my .tab .item {
  flex: 1;
  position: relative;
  height: 70px;
  text-align: center;
}
body.my .tab .item[data-count]:after {
  content: attr(data-count);
  display: block;
  width: 14px;
  height: 14px;
  line-height: 14px;
  background-color: #f00;
  border-radius: 7px;
  font-size: 14px;
  color: #fff;
  position: absolute;
  right: 26px;
  top: 4px;
}
body.my .tab .item a {
  display: inline-block;
  height: 40px;
  width: 50px;
  background: url("../image/my-tab.png") no-repeat;
  background-size: 380px auto;
  background-position-y: 6px;
}
body.my .tab .item a.unpaid {
  background-position-x: 0;
}
body.my .tab .item a.receiving {
  background-position-x: -106px;
}
body.my .tab .item a.address {
  background-position-x: -220px;
}
body.my .tab .item a.order {
  background-position-x: -336px;
}
body.my .tab .item p {
  font-size: 14px;
  color: #333;
}
body.my .order-list {
  background-color: #fff;
}
body.my .order-list .order-item {
  height: 100px;
  padding: 5px 10px;
  display: flex;
}
body.my .order-list .order-item .prod-img {
  flex: 1;
}
body.my .order-list .order-item .prod-img img {
  width: 90px;
  height: 90px;
  border: 1px solid #ddd;
  display: inline-block;
  vertical-align: middle;
}
body.my .order-list .order-item .prod-info {
  display: inline-block;
  height: 90px;
  width: 140px;
  vertical-align: middle;
  flex: 2;
  padding: 5px;
  font-size: 14px;
  word-break: break-all;
  position: relative;
}
body.my .order-list .order-item .prod-info .p {
  position: absolute;
  bottom: 5px;
  font-size: 16px;
  color: #f00;
}
body.my .order-list .order-item .op {
  flex: 1;
  text-align: center;
  border-left: 1px solid #ddd;
  line-height: 90px;
  font-size: 14px;
}
.cate {
  background-color: #eee;
  overflow: hidden;
}
.cate .right {
  z-index: 1;
  overflow: hidden;
  width: 73%;
  max-height: 100%;
  float: right;
}
.cate .right .cate-item {
  padding: 10px 5px;
  background-color: #fff;
  margin-bottom: 10px;
}
.cate .right .cate-item .hd {
  padding-bottom: 10px;
  color: #067cca;
}
.cate .right .cate-item .hd:before {
  content: ' ';
  background: url(../image/cate_flag.png) no-repeat;
  padding: 0px 12px;
  background-size: 20px;
}
.cate .right .cate-item .content .row {
  display: flex;
  padding-bottom: 10px;
}
.cate .right .cate-item .content .row div {
  flex: 1;
  text-align: center;
}
.cate .right .cate-item .content .row div img {
  width: 80px;
  height: 80px;
  border: 1px solid #eee;
}
.cate .right .cate-item .content .row div p {
  font-size: 14px;
}
.cate .left {
  z-index: 1;
  width: 25%;
  float: left;
  background-color: #fff;
  overflow: hidden;
  height: 100%;
}
.cate .left:after {
  content: ' ';
  right: 0;
  top: 0;
  position: absolute;
  height: 100%;
  background-color: #ccc;
  width: 1px;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
}
.cate .left .c {
  text-align: center;
  font-size: 14px;
  padding: 15px 0;
  position: relative;
}
.cate .left .c:before {
  content: ' ';
  bottom: 0;
  left: 0;
  position: absolute;
  height: 1px;
  background-color: #ccc;
  width: 100%;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.cate .left .c.active {
  color: #067cca;
  border-left: 5px solid #067cca;
}
html,
body {
  width: 100%;
  height: 100%;
}
input[type=search]::-webkit-search-cancel-button {
  display: none;
}
