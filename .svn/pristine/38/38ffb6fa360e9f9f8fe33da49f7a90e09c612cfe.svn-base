@charset "utf-8";
/* CSS Document */

/*头部logo部分*/
.logo {
	margin-left: 77px;
	margin-top: 
}
#header .buy {
	margin-top: 0px;
}
#header {
	margin-bottom: 30px;
	padding-top: 40px;
}

/*内容部分*/
#contents .inner {
	background: #fff;
	padding: 20px 20px;
}
#contents .inner:after {
  clear: both;
  content: ".";
  display: block;
  height: 0;
  visibility: hidden;
}
/*左边导航*/
#side_navi {
	float: left;
	width: 202px;
}
#side_navi dt {
	padding: 8px 10px 8px 15px;
	color: #FFF;
	font-size: 13px;
	font-weight: bold;
	position: relative;
}
#side_navi dt.tlt_merchandise {
	background: #0091dc;
}
#side_navi dt.tlt_information {
	background: #0091dc;
}
#side_navi dt.tlt_transaction {
	background: #0091dc;
}
#side_navi dt.tlt_reeturns {
	background: #0091dc;
}
#side_navi dt.tlt_receipt {
	background: #0091dc;
}
#side_navi dt span {
	position: absolute;
	right: 10px;
	top: 10px;
	font-size: 15px;
}
#side_navi dd {
	padding: 10px 10px 0 15px;
	font-size: 12px;
	border-right: 1px solid #dfdfdf;
	border-left: 1px solid #dfdfdf;
}
#side_navi dd.last_child {
	border-bottom: 1px solid #dfdfdf;
}
#side_navi dd span {
	padding: 0 5px 0 0;
	color: #0091dc;
	font-size: 13px;
}
#side_navi dd ul li {
	padding: 0 0 10px;
}
#side_navi dd ul li.active a {
	font-weight: bold;
	color: #ff4200;
}
/*右边内容*/
#matter_section {
	float: right;
	width: 950px;
	min-height: 400px;
}
.inner {
	font-size: 12px;
	
}
#matter_section .list_inner {
	padding: 20px 30px 5px;
	border-bottom: 1px dotted #dfdfdf;
	background: #f6f4f5;
}
/*.nav_step*/
.nav_step  {
	margin-bottom: 20px;
}
.nav_step ul li {
	float:left; 
	height:35px; 
	padding-right:20px; 
	line-height:35px; 
	background:url(../img/bid/img-step.png) no-repeat 100% 0 #c9c9c9; 
	font-weight:bold;
}
.nav_step ul li a {
	display: block;
	height: 35px;
	width: 170px;
	font-size: 14px;
	color: #fff;
	padding-left: 10px;
}
.nav_step ul li a span {
	display: inline-block;
	vertical-align: middle;
	margin-top: -5px;
	margin-right: 10px;
}
.nav_step ul li.done {
	color:#fff;
	background-position:100% -70px;
	background-color:#565656;
}
.nav_step ul li.current_prev {
	color:#fff; 
	background-position:100% -35px;
	background-color:#565656;
}
.nav_step ul li.done a, .nav_step ul li.current_prev a {
	background:#565656;
}
.nav_step  ul li.current {
	color:#fff;
	background-color:#0091dc;
}
.nav_step  ul li.current span{
	background:#0091dc;
}
.nav_step  ul li.last {
	background-position:100% -105px;
	background-color:transparent;
}
.nav_step  ul li.last a {
	background:#c9c9c9;
}
.nav_step  ul li.last.current {
	background-position:100% -140px;
}.nav_step  ul li.last.current a {
	background: #0091dc;
}
.nav_step  ul li.last.current span {
	background:#0091dc;
}

/*表单内容*/
.step_section .form-group {
	width: 425px;
	float: left;
	margin: 0 10px 10px;
}
.step_section .form-group label.title {
	width: 90px;
	text-align: right;
	padding-right: 5px;
	float: left;
	line-height: 28px;
}
.step_section .form-group .input_area  {
	width: 335px;
	float: left;
}
.step_section .form-group .form-control {
	height: 28px;
    padding: 0 0 0 5px;
	font-size: 12px;
}
.step_section .input-group span {
    display: table-cell;
    padding-left: 5px;
    vertical-align: middle;
}
.step_section .form-group span {
	line-height: 28px;
}
.step_section .special_group01 {
	width: 890px;
}
.step_section .special_group01 textarea.form-control {
	height: auto;
}
/*表格*/
.add_inner table {
	width: 950px;
	margin-top: 10px;
	
}
.add_inner table tr td.left {
	text-align: left;
}
.add_inner table tr th {
	background: #dfdfdf;
	border:1px solid #dfdfdf;
	border-right: 1px solid #fff;
	line-height: 28px;
	text-align: center;
	font-size: 12px;
}
.add_inner table tr th.last {
	border-right: 1px solid #dfdfdf;
}
.add_inner table tr td {
	border:1px solid #dfdfdf;
	padding: 6px;
	font-size: 12px;
	vertical-align: top;
	text-align: center;
}
.add_inner table tr td a {
    color: #0089d1;
    margin: 0 2px;
}
.add_inner table tr td a:hover {
	color: #ff4200;
}
.add_inner table tr td button {
	height: 20px;
	line-height: 20px;
}
/*右对齐按钮*/
.add_inner .btn_list {
	text-align: right;
	margin-top: 10px;
}
.add_inner button {
	display: inline-block;
}
.add_inner .title_add {
	text-align: right;
}
.add_inner .title_add .title_inner {
	display: inline-block;
}
.add_inner .title_add p {
	float: left;
	line-height: 28px;
	margin: 10px 10px 0 0;
	font-weight: bold;
}
.add_inner .title_add p span {
	font-weight: 100;
	margin-left: 10px;
}
.add_inner .title_add .btn_list {
	float: right;
}
.add_inner_first {
	border-bottom: 1px dotted #dfdfdf;
	padding-bottom: 20px;
}


/*居中对齐按钮*/
.submit_btn {
	text-align: center;
}
.submit_btn ul {
	display: inline-block;
}
.submit_btn ul li {
	float: left;
	margin: 30px 10px 30px;
}






/*弹出框*/
.modal-header {
	background: #0091dc;
	color: #fff;
	border-radius: 5px 5px 0 0;
	padding: 10px;
}
.modal-header h4 {
	text-align: left;
}
.modal-header .close {
	color: #fff;
}
.modal-header .radio-inline, .checkbox-inline {
	padding-left: 0;
}

.input-append {
	margin-top: 20px;
}
.input-append label {
	margin-left: 5px;
	font-weight: 100;
}
.modal-body .form-group {
	margin: 0;
	float: none;
	width: 100%;
	margin-bottom: 10px;
}
.modal-body .form-group label.col-sm-2 {
	margin-top: 5px;
}
.modal-body .form-group .col-sm-2 {
	padding-left: 0;
	padding-right: 10px;
	text-align: right;
}
.modal-body .form-group .col-sm-4,
.modal-body .form-group .col-sm-10 {
	padding-left: 0;
	padding-right: 0;
	text-align: left;
}

.modal-body .form-group textarea.form-control {
	height: auto;
}
.modal-body .form-group .form-control {
	width: 100%;
}
.modal-body .input-group .form-control {
	height: 28px;
}
@media (max-width: 768px) {
  .modal-body .form-group label {
	  text-align: left;
	  padding-left: 0;
	  margin-bottom: 0;
  }
}
.modal-body .form-group label {
}
.modal-body table {
	width: 100%;
	margin-top: 20px;
}
.modal-footer {
	clear: both;
}
/*分页*/
.page {
	text-align: right;
}
.pagination > li > a, .pagination > li  {
	color: #666666;
}
.pagination > li.active > a,
.pagination > li.active > a:hover,
.pagination > li > a:hover,
.pagination > li > a:focus {
  color: #fff;
  background-color: #0091dc;
  border-color: #0091dc;
}
/*按钮*/
.blue {
	background: #0091dc;
	color: #fff;
	border: none;
	height: 28px;
	padding: 0 12px;
	line-height: 28px;
}
.blue:hover {
	background: #dfdfdf;
}
.gray_cart {
	background: #666;
	border: none;
	color: #fff;
	height: 28px;
	padding: 0 12px;
	line-height: 28px;
}
/*表格*/
.table_area{
	width: 100%;
	margin-top: 20px;
	
}
.table_area tr td.left {
	text-align: left;
}
.table_area tr th {
	background: #dfdfdf;
	border:1px solid #dfdfdf;
	border-right: 1px solid #fff;
	line-height: 28px;
	text-align: center;
	font-size: 12px;
}
.table_area tr th.last {
	border-right: 1px solid #dfdfdf;
}
.table_area tr td {
	border:1px solid #dfdfdf;
	padding: 6px;
	font-size: 12px;
	vertical-align: middle;
	text-align: center;
}
.table_area tr td a {
    color: #0089d1;
    margin: 0 2px;
}
.table_area tr td a:hover {
	color: #ff4200;
}
.table_area tr td button {
	height: 20px;
	line-height: 20px;
}