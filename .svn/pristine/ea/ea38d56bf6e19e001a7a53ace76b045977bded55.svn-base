  height: 34px;
}
body.search .order {
  z-index: 5;
  width: 100%;
  height: 38px;
  display: flex;
  top: 46px;
  background-color: #fff;
}
body.search .order li {
  flex: 1;
  text-align: center;
  line-height: 38px;
  font-size: 14px;
  position: relative;
}
body.search .order li.full-right-border:before {
  height: 100%;
  top: 0;
}
body.search .order li:before {
  content: ' ';
  left: 0;
  top: 8px;
  position: absolute;
  background-color: #ddd;
  width: 1px;
  height: 22px;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
  -webkit-transition: all ease .3s;
  transition: all ease .3s;
}
body.search .order li:after {
  content: ' ';
  left: 0;
  bottom: -1px;
  position: absolute;
  height: 1px;
  background-color: #ddd;
  width: 100%;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
body.search .order li:nth-of-type(1):before {
  display: none;
}
body.search .order li.active {
  color: #067cca;
}
body.search .order li.active:after {
  width: 0;
}
body.search .order li.active span:after {
  color: #067cca;
  content: '\e728';
}
body.search .order li span:after {
  font-family: iconfont;
