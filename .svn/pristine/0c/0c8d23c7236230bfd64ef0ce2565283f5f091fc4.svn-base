@charset "utf-8";
/* CSS Document */
.btn_add ul li {
	float: left;
	line-height: 25px;
	margin-right: 8px;
}
#matter_section ul li {
	float: left;
	line-height: 25px;
	margin-right: 8px;
}
#matter_section .special_input {
	border-right: none;
}
#matter_section .form-control {
	height: 28px;
	width: 115px;
}
#matter_section .input-group-addon {
	height: 28px;
	padding: 7px 5px 5px 0px;
	border-left: none;
	background: #fff;
	cursor: pointer;
	display: inline-table;
}
.btn {
	height: 28px;
	line-height: 28px;
	padding: 0 20px;
	border: none;
}
.red {
	background: #db433d;
	color: #fff;
}
.gray {
	background: #666666;
	color: #fff;
}
.btn-list {
	text-align: right;
}
.btn_add ul {
	display: inline-block;
}
.search_result table tr td {
	text-align: center;
}
.search_result table tr td.address{
	text-align: left;
}
.search_result table tr td a{
	margin: 0 2px;
	color: #0089d1;
}
.search_result table tr td a:hover {
	text-decoration: underline;
}
.add_details {
	width: 730px;
	margin: 0 auto;
	border: 1px solid #dfdfdf;
	background: #f6f5f5;
	margin-top: 25px;
	padding: 30px 75px;
}
.add_details dl {
	margin-bottom: 10px;
}
.add_details dt {
	float: left;
	line-height: 28px;
	text-align: right;
	width: 95px;
}
.add_details dd {
	float: left;
	width: 460px;
	margin-left: 5px;
}


.add_details .form-control {
	height: 28px;
}
.add_details select {
	display: inline-block;
	width: 150px;
	padding: 0;
	}
.add_details span.red_remind {
	display: inline;
	color: #db433d;
	margin-right: 10px;
}
.btn_middle {
	text-align: center;
	margin-top: 30px;
}
#contents .details{
	padding-bottom: 60px;
}
.check dd {
	line-height: 28px;
}