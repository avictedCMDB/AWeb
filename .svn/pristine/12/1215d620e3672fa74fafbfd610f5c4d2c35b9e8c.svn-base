@charset "utf-8";
/* CSS Document */
body {
	margin: 0;
	padding: 0;
}
* {
  -webkit-box-sizing: border-box;
     -moz-box-sizing: border-box;
          box-sizing: border-box;
}
*:before,
*:after {
  -webkit-box-sizing: border-box;
     -moz-box-sizing: border-box;
          box-sizing: border-box;
}
* {
	padding: 0;
	margin:0;
}
html {
  font-size: 10px;

  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
body {
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 16px;
  line-height: 1.42857143;
  color: #333; 
}
input,
button,
select,
textarea {
  font-family: inherit;
  font-size: inherit;
  line-height: inherit;
}
a {
  color: #666666;
  text-decoration: none;
}
a:hover,
a:focus {
  color: #db433d;
  text-decoration: none;
}
a:focus {
  outline: thin dotted;
  outline: 5px auto -webkit-focus-ring-color;
  outline-offset: -2px;
}
ul,ol {
	margin: 0;
}
ul li,ol li {
	list-style: none;
}
.clearfix:before,
.clearfix:after{
  display: table;
  content: " ";
}
.clearfix:after {
  clear: both;
}



.page-wrapper {
	background: url(../img/bg.gif) left top repeat;
}
#header_top {
	border-bottom: 3px solid #fff;
}
.inner {
	width: 1210px;
	margin: 0 auto;
}
#header_top .social-links {
	list-style: none;
	font-size: 0;
	padding: 0;
	text-align: right;
}
#header_top .social-links li {
	display: inline-block;
	margin: 14px 2px 16px 20px;
	border-right: 1px solid #666;
	padding-right: 15px;
}
#header_top .social-links li a {
	display: block;
	text-align: center;
	font-size: 12px;
}
#header_top .social-links li span {
	display: inline-block;
	margin: 0 8px;
}
#header {
	background: url(../img/bg-header.png) top center no-repeat;
	padding-top: 20px;
}
#header .logo {
	float: left;
}
#header .search {
	float: left;
	width: 537px;
	margin-left: 30px;
}
#header .search .search_cate ul li {
	float: left;
}
#header .search_cate {
	list-style: none;
	padding: 0;
	margin-bottom: 0;
	margin-top: 13px;
}
#header .search_cate li{
	display: inline-block;
	float: left;
}
#header .search_cate li a {
	display: block;
	height: 25px;
	line-height: 25px;
	padding: 0px 20px;
}
#header .search_cate li.active a,
#header .search_cate li a:hover {
	background: #db433d;
	color: #fff;
}

#header .form-control {
	border-radius: 0;
	border: 2px solid #db433d;
	height: 40px;
}
#header .btn-default {
	height: 40px;
	border: 2px solid #db433d;
	background: #db433d;
	color: #fff;
	font-size: 16px;
	padding: 0 20px;
}
#header .btn-default:hover,
#header .btn-default:focus,
#header .btn-default:active {
	background: #db433d;
	text-decoration: underline;
	color: #fff;
	border: 2px solid #db433d;
}
#header .buy .btn {
	position: relative;
	width: 172px;
}
#header .buy {
	height: 40px;
	width: 172px;
	margin-left: 30px;
	float:left;
	margin-top: 38px;
}
#header .buy i {
	font-size: 18px;
}
#header .shopping {
	position: absolute;
	left: 5px;
	top: 8px
}
#header .arrow-right {
	position: absolute;
	right: 5px;
	top: 8px;
}
#nav_area {
	border-bottom: 3px solid #db433d;
	margin-top: 25px;
}
#nav_area ul.navitems {
	float: left;
}
#nav_area ul.navitems li {
	display: block;
	float: left;
	text-align: center;
	height: 40px;
}
#nav_area .categorys {
	width: 220px;
	display: block;
	float: left;
	text-align: center;
	height: 34px;
	position: relative;
}
#nav_area .categorys .title a,
#nav_area .navitems li a {
	display: block;
	padding: 0px 50px;
	font-size: 16px;
	color: #db433d;
	height: 34px;
	line-height: 34px;
	font-weight: bold;
}
#nav_area .categorys .title a,
#nav_area .navitems li.active a,
#nav_area .navitems li a:hover {
	background: #db433d;
	color: #fff;
	
}
#nav_area .categorys .cate_inner {
	margin-top: 3px;
}
#nav_area .categorys .cate_inner li {
	height: 37px;
	overflow: hidden;
	line-height: 37px;
	display: block;
}
#nav_area .categorys .cate_inner li + li {
	margin-top: 0;
}
#nav_area .categorys .cate_inner li a{
	border-radius: 0;
	padding-top: 0;
	padding-bottom: 0;
	background: #fff;
	color: #777777;
	font-weight: 100;
	text-align: left;
	font-size: 14px;
	width: 220px;
}
#nav_area .categorys .cate_inner li a:hover,
#nav_area .categorys .cate_inner li a:focus,
#nav_area .categorys .cate_inner li.active a, #nav_area .categorys .cate_inner li.active a:hover, #nav_area .categorys .cate_inner li.active a:focus {
    color: #db433d;
	background: #fff;
}
#nav_area .categorys .cate_inner li.second a {
	background: #f1f1f1;
}
#nav_area .item_inner .title ul > li > a:after,
.nav-pills.nav-stacked > li > a:after {
	font-family: "FontAwesome";
	content: "\f105";
	font-size: 14px;
	font-weight: 700;
	position: absolute;
	right: 15px;
	top: 0px;
	-webkit-transition: all 0.2s ease-in-out;
	-moz-transition: all 0.2s ease-in-out;
	-o-transition: all 0.2s ease-in-out;
	-ms-transition: all 0.2s ease-in-out;
	transition: all 0.2s ease-in-out;
}


#nav_area .item_inner {
	width: 990px;
	position: absolute;
	left: 220px;
	overflow: hidden;
	top: 43px;
	z-index: 9999;
	background: #fff;
	padding: 20px 0 0 20px;
	min-height: 440px;
	border: 1px solid #db433d;
	border-left: none;
	border-top: none;
	display: none;
}
a img:hover {
	opacity: 0.7;
}
#nav_area .item_inner .title {
	display: block;
	
}
#nav_area .item_inner ul {
	margin-left: 0;

}
#nav_area .item_inner ul li {
	float: left;
	list-style: none;
}
#nav_area .item_inner .title ul li a {
	padding: 0 20px;
	text-align: center;
	margin-right: 10px;
	background: #656565;
	border-radius: 4px;
	color: #fff;
	position: relative;
	font-size: 12px;
	height: 25px;
	line-height: 25px;
}
#nav_area .item_inner .title ul li a:hover, .son_nav ul li a:focus {
	color: #fff;
	background: #db433d;
}
#nav_area .item_inner .title ul > li > a:after {
	position: absolute;
	right: 10px;
	top: 0px;
}
#nav_area .item_inner .nav_list dl {
	border-bottom: 1px dashed #dfdfdf;
	margin-bottom: 0;
	font-size: 12px;
}
#nav_area .item_inner .nav_list dl dt{
	line-height: 40px;
	text-align: right;
	width: 150px;
	float: left;
}
#nav_area .item_inner .nav_list dl dd {
	padding-top: 5px;
	float: left;
	margin-left: 20px;
	width: 700px;
}
#nav_area .item_inner .nav_list dl dd li {
	line-height: 30px;
	height: auto;
	margin-right: 30px;
}



#footer {
	background: #e9e9e9;
}
#footer .inner {
	background:  url(../img/bg-header.png) top center no-repeat;
	padding: 20px 0px;
}
#footer .inner .tel {
	width: 312px;
	padding: 25px 0;
	margin-left: 73px;
	float: left;
	margin-right: 73px;
}
#footer .section {
	background: url(../img/bg-footer.png) top left repeat-y;
	
}
#footer .customer_service {
	float: left;
	width: 752px;
	padding: 25px 0 0;
}
#footer .customer_service dl {
	float: left;
	width: 220px;
	color: #666666;
}
#footer .customer_service dl dt {
	padding-left: 20px;
}
#footer .customer_service dl dt.icon_shopping {
	background: url(../img/icon-shopping.jpg) left center no-repeat;
}
#footer .customer_service dl dt.icon-delivery {
	background: url(../img/icon-delivery.jpg) left center no-repeat;
}
#footer .customer_service dl dt.icon-other {
	background: url(../img/icon-other.jpg) left center no-repeat;
}
#footer .customer_service dl dd {
	padding-left: 30px;
	font-size: 12px;
}
#footer .customer_service dl dd li {
	margin-top: 5px;
	list-style: disc;
}
#copy_right {
	background: #3c3d42;
}
#copy_right .inner {
	text-align: center;
	color: #656764;
	line-height: 57px;
	font-size: 12px;
}
.main_cate {
	z-index: 9999;}